            <xsd:element name="eentryTime" type="xsd:dateTime" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Earliest entry time in the FXA.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="initialGateDepartureTime" type="xsd:dateTime" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Initial Gate Departure Time.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="edctSubShowReportType">
    <xsd:annotation>
      <xsd:documentation>EDCT Sub Show report</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="controlledElement" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>Controlled element.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="controlledElementName" type="fce:controlElementNameType">
              <xsd:annotation>
                <xsd:documentation>Controlled element name.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="controlledElementType" type="fce:controlElementTypeType">
              <xsd:annotation>
                <xsd:documentation>Controlled element program type.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="subsFlag" type="nce:flagType">
              <xsd:annotation>
                <xsd:documentation>Substitution indicator.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="scsFlag" type="nce:flagType">
              <xsd:annotation>
                <xsd:documentation>SCS indicator.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="acFlag" type="nce:flagType">
              <xsd:annotation>
                <xsd:documentation>AC indicator.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="bridgingOffAirline" type="nce:airlineIdType" minOccurs="0" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Bridging Off airlines.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="gaBridgingFlag" type="nce:flagType">
        <xsd:annotation>
          <xsd:documentation>General Aviation bridging indicator.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="militaryBridgingFlag" type="nce:flagType">
        <xsd:annotation>
          <xsd:documentation>Military bridging indicator.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="edctUnassignedSlotsReportType">
    <xsd:annotation>
      <xsd:documentation>EDCT Unassigned Slots report</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="controlledElementName" type="fce:controlElementNameType">
        <xsd:annotation>
          <xsd:documentation>Controlled element name.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="slot" type="fce:slotNameType" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>Slot list.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="fxaDeleteType">
    <xsd:annotation>
      <xsd:documentation>FEA/FCA delete request type.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="fca_id" type="fcm:fcaIdType" maxOccurs="100">
        <xsd:annotation>
          <xsd:documentation>Unique Identifiers of the FCAs. Each identifier will be separated by a space up to  a maximum of 100 for FCADelete messages.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="fcaUpdateMessageType">
    <xsd:annotation>
      <xsd:documentation>The FCA Update Message is used to initiate a database update when creating a new FCA/FEA or editing an existing FCA/FEA.</xsd:documentation>
    </xsd:annotation>
    <xsd:choice>
      <xsd:element name="FCA" type="fxaDefinitionType">
        <xsd:annotation>
          <xsd:documentation>XML-tagged data elements. The XML tagged portion of the message includes data elements that define the FCA as well as data elements for the primary filter and the data elements for the Timeline and Dynamic List preferences.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SECONDARY_FILTER" type="fm2:fxaSecondaryFilterType">
        <xsd:annotation>
          <xsd:documentation>Provides the secondary filter.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="fxaDefinitionType">
    <xsd:annotation>
      <xsd:documentation>Defines the parameters associated with a FEA/FCA TMI.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="FCA_ID" type="fcm:fcaIdType">
        <xsd:annotation>
          <xsd:documentation>Identifier of FEA/FCA with which this filter is associated.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="NAME" type="fcm:fcaNameType">
        <xsd:annotation>
          <xsd:documentation>User-assigned filter name (must be unique among all filters associated with the same FEA/FCA).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DOMAIN" type="nce:tmiVisibilityDomainType">
        <xsd:annotation>
          <xsd:documentation>Domain of filter. Possible values are PUBLIC, SHARED, LOCAL, or PRIVATE.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="LASTUPDATE" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Date and time1 filter was last updated.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="UP_WKSTN" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>TFMS workstation from which the FEA/FCA was last updated</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="UP_SITE" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>TFMS site from which the FEA/FCA was last updated</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CR_WKSTN" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>TFMS site from which the FEA/FCA was created</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CR_SITE" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>TFMS workstation from which the FEA/FCA was created</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="REASON">
        <xsd:annotation>
          <xsd:documentation>Reason for the FEA/FCA; one of NONE, WEATHER, VOLUME, RUNWAY, EQUIPMENT, OTHER.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="NONE"/>
            <xsd:enumeration value="WEATHER"/>
            <xsd:enumeration value="VOLUME"/>
            <xsd:enumeration value="RUNWAY"/>
            <xsd:enumeration value="EQUIPMENT"/>
            <xsd:enumeration value="OTHER"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="TYPE">
        <xsd:annotation>
          <xsd:documentation>One of FEA, FCA.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="FEA"/>
            <xsd:enumeration value="FCA"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="COLOR_ID">
        <xsd:annotation>
          <xsd:documentation>Color index indicating the color that TFMS is using to draw the filter. 0 - 35.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:int">
            <xsd:maxInclusive value="35"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="START" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Date and time for starting the FEA/FCA</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="END" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Date and time for ending the FEA/FCA</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FSM_ELIGIBLE">
        <xsd:annotation>
          <xsd:documentation>Flag to indicate whether FEA/FCA is eligible to be monitored by FSM.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="TRUE"/>
            <xsd:enumeration value="FALSE"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="EXTENDED">
        <xsd:annotation>
          <xsd:documentation>Flag to indicate whether the FEA is an Extended FEA.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="TRUE"/>
            <xsd:enumeration value="FALSE"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="LOOK_AHEAD">
        <xsd:annotation>
          <xsd:documentation>Time range in hours from current time over which TFMS will determine the FCA flights. Content is 1-15 hours.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:int">
            <xsd:minInclusive value="1"/>
            <xsd:maxInclusive value="24"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="SHARE_SITES" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Space-separated list of TFMS sites with which the FEA/FCA was shared.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction>
            <xsd:simpleType>
              <xsd:list itemType="fce:siteType"/>
            </xsd:simpleType>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:choice>
        <xsd:element name="POLYGON" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>Polygon definition.</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CEILING" type="fcm:fcaCeilingType">
                <xsd:annotation>
                  <xsd:documentation>Ceiling altitude.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="FLOOR" type="fcm:fcaFloorType">
                <xsd:annotation>
                  <xsd:documentation>Floor altitude.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="DIRECTION" type="fcm:fcaDirectionType">
                <xsd:annotation>
                  <xsd:documentation>Direction/bearing.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="SPEED" type="fcm:fcaSpeedType">
                <xsd:annotation>
                  <xsd:documentation>Speed.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="DRAWING" type="fcm:fcaDrawingType">
                <xsd:annotation>
                  <xsd:documentation>Geometric data provided indicator.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="POINTS" type="fcm:fcaPointType">
                <xsd:annotation>
                  <xsd:documentation>The points defining the horizontal projection of the polygon.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="LINE" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>Provides the parameters for a Line type FEA/FCA.</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CEILING" type="fcm:fcaCeilingType">
                <xsd:annotation>
                  <xsd:documentation>Ceiling altitude.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="FLOOR" type="fcm:fcaFloorType">
                <xsd:annotation>
                  <xsd:documentation>Floor altitude.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="DIRECTION" type="fcm:fcaDirectionType">
                <xsd:annotation>
                  <xsd:documentation>Direction/bearing.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="SPEED" type="fcm:fcaSpeedType">
                <xsd:annotation>
                  <xsd:documentation>Speed.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="DRAWING" type="fcm:fcaDrawingType">
                <xsd:annotation>
                  <xsd:documentation>Geometric data provided indicator.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="POINTS" type="fcm:fcaPointType">
                <xsd:annotation>
                  <xsd:documentation>The points defining the horizontal projection of the polygon.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="CIRCLE" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>Provides the parameters for a Circle type FEA/FCA.</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="CEILING" type="fcm:fcaCeilingType">
                <xsd:annotation>
                  <xsd:documentation>Ceiling altitude.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="FLOOR" type="fcm:fcaFloorType">
                <xsd:annotation>
                  <xsd:documentation>Floor altitude.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="RADIUS">
                <xsd:annotation>
                  <xsd:documentation>Radius of circle( Nautical Miles)</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                  <xsd:restriction base="xsd:int">
                    <xsd:minInclusive value="0"/>
                    <xsd:maxInclusive value="999"/>
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:element>
              <xsd:element name="CENTER">
                <xsd:annotation>
                  <xsd:documentation>Center Point of circle.</xsd:documentation>
                </xsd:annotation>
                <xsd:simpleType>
                  <xsd:restriction base="xsd:string">
                    <xsd:pattern value="\d{4}[NS][/]\d{5}[EW]"/>
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="adaptedAirspace" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>Predefined/adapted airspace.</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:choice>
                <xsd:element name="AIRPORT" type="nce:airportIdType">
                  <xsd:annotation>
                    <xsd:documentation>Indicates that adapted element category is an airport. Content is airport name.</xsd:documentation>
                  </xsd:annotation>
                </xsd:element>
                <xsd:element name="SECTOR">
                  <xsd:annotation>
                    <xsd:documentation>Indicates that adapted element category is sector in its dynamic state. Content is sector name.</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                      <xsd:pattern value="[A-Z]{1}[A-Z;0-9]{2,5}"/>
                    </xsd:restriction>
                  </xsd:simpleType>
                </xsd:element>
                <xsd:element name="BASE_SECTOR">
                  <xsd:annotation>
                    <xsd:documentation>Indicates that adapted element category is sector in its baseline state. Content is sector name.</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                      <xsd:pattern value="[A-Z]{1}[A-Z;0-9]{2,5}"/>
                    </xsd:restriction>
                  </xsd:simpleType>
                </xsd:element>
                <xsd:element name="TRACON">
                  <xsd:annotation>
                    <xsd:documentation>Indicates that adapted element category is TRACON in its dynamic state. Content is TRACON name.</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                      <xsd:pattern value="\w{3,6}"/>
                    </xsd:restriction>
                  </xsd:simpleType>
                </xsd:element>
                <xsd:element name="ARTCC">
                  <xsd:annotation>
                    <xsd:documentation>Indicates that adapted element category is ARTCC. Content is ARTCC name.</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                      <xsd:pattern value="\w{3}"/>
                    </xsd:restriction>
                  </xsd:simpleType>
                </xsd:element>
                <xsd:element name="FIX" type="nce:fixNameType">
                  <xsd:annotation>
                    <xsd:documentation>Indicates that adapted element category is fix. Content is fix name.</xsd:documentation>
                  </xsd:annotation>
                </xsd:element>
                <xsd:element name="SUA" type="xsd:string">
                  <xsd:annotation>
                    <xsd:documentation>Indicates that adapted element category is special use area. Content is SUA name.</xsd:documentation>
                  </xsd:annotation>
                </xsd:element>
              </xsd:choice>
              <xsd:element name="CEILING" type="fcm:fcaCeilingType" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Ceiling altitude.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="FLOOR" type="fcm:fcaFloorType" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Floor altitude.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
      <xsd:element name="PRIMARY_FILTER" type="fxaPrimaryDefinitionType">
        <xsd:annotation>
          <xsd:documentation>Primary filter definition.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FCA_DISPLAY_PREFERENCES" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Display preferences.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="FCA_TIMELINE">
              <xsd:annotation>
                <xsd:documentation>The FEA/FCA timeline.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="VISIBLE_TIME_RANGE">
                    <xsd:annotation>
                      <xsd:documentation>Default visible time range of Timeline, in minutes</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:int">
                        <xsd:minInclusive value="60"/>
                        <xsd:maxInclusive value="330"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="SELECTED_TIME_RANGE">
                    <xsd:annotation>
                      <xsd:documentation>Default time range of selected intervals for displaying flights and flight data, in minutes.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:int">
                        <xsd:minInclusive value="15"/>
                        <xsd:maxInclusive value="1440"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="FLIGHT_COUNT_UNIT">
                    <xsd:annotation>
                      <xsd:documentation>Default selection rule for flight count values</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="PEAK"/>
                        <xsd:enumeration value="TOTAL"/>
                        <xsd:enumeration value="ENTRY"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element name="LIST_SORT">
              <xsd:annotation>
                <xsd:documentation>Sort list.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="CTR_SORT_LEVEL" maxOccurs="3">
                    <xsd:annotation>
                      <xsd:documentation>Center sort.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="SORT_LEVEL">
                          <xsd:annotation>
                            <xsd:documentation>Sort level.</xsd:documentation>
                          </xsd:annotation>
                          <xsd:simpleType>
                            <xsd:restriction base="xsd:int">
                              <xsd:enumeration value="1"/>
                              <xsd:enumeration value="2"/>
                              <xsd:enumeration value="3"/>
                            </xsd:restriction>
                          </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="COLUMN_HDR" type="fcaDisplayColumnHeaderType" minOccurs="0">
                          <xsd:annotation>
                            <xsd:documentation>Column to be sorted.</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="SORT_ORDER">
                          <xsd:annotation>
                            <xsd:documentation>Order for sorting the column</xsd:documentation>
                          </xsd:annotation>
                          <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                              <xsd:enumeration value="ASCENDING"/>
                              <xsd:enumeration value="DESCENDING"/>
                            </xsd:restriction>
                          </xsd:simpleType>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element name="CTR_COLUMN_DISPLAY">
              <xsd:annotation>
                <xsd:documentation>Center column display.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="COLUMN_HDR" type="fcaDisplayColumnHeaderType" maxOccurs="unbounded">
                    <xsd:annotation>
                      <xsd:documentation>Name of the column data item. Columns are listed in the order to be displayed. One of "Aircraft ID", "Secondary Filter", "Aircraft Type", "NRP Indicator", "FCA Indicator", "Airline", "Departure Time", "Arrival Time", "Controlled Arrival Time", Controlled Departure Time", "Control Element", "Filed Speed", "Origin", "Destination", "Full Route", "Abbreviated Route", "Time to  Intersect", "Entry Time", "Exit Time", "Requested Altitude", "Current Altitude", "Departure Center", "RVSM".</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="fxaPrimaryDefinitionType">
    <xsd:annotation>
      <xsd:documentation>Defines FEA/FCA Filters.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="COLOR_ID" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Color index indicating the color that TFMS is using to draw the filter. 0 - 35. (NOTE: See Appendix A for a definition of the COLOR_ID values.)</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:int">
            <xsd:maxInclusive value="35"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="CONDITIONS">
        <xsd:annotation>
          <xsd:documentation>Conditions.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:all>
            <xsd:element name="ANY" type="fcm:primaryAnyFcaFilterType" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>ANY filter.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="ALL" type="fcm:primaryAllFcaFilterType" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>ALL filter.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="flightIdRangeType">
    <xsd:annotation>
      <xsd:documentation>Range of flightId values.</xsd:documentation>
    </xsd:annotation>
    <xsd:attribute name="airlineId" type="nce:airlineIdType" use="required">
      <xsd:annotation>
        <xsd:documentation>Three-letter airline code.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="min" use="required">
      <xsd:annotation>
        <xsd:documentation>Flight number range minimum. Can be no less than 0 and no higher than 9999.</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:integer">
          <xsd:pattern value="[0-9]{1,4}"/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="max" use="required">
      <xsd:annotation>
        <xsd:documentation>Flight number range Maximum. Can be no less than 0 and no higher than 9999.</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:integer">
          <xsd:pattern value="[0-9]{1,4}"/>
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:simpleType name="computerCIDType">
    <xsd:annotation>
      <xsd:documentation>This is the host computer-assigned identification. A ddL format will normally be used only when all numbers of the ddd format are in use. The letters I and O will not be used. This identification is only unique within the assigning facility.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9F]{2}[a-hj-np-zA-HJ-NP-Z0-9]"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="coordinationPointType">
    <xsd:annotation>
      <xsd:documentation>NAS Field Reference: Field 06. NAS Format: This field may take the form of the following types: latLongType, fixNameType, fixRadialDistanceType.  Refer to these types
        for allowable formats. A fix mutually agreed upon by the facilities concerned for the coordination of center control transfer.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="fm2:filedFixType"/>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="flightPlanMessageType">
    <xsd:annotation>
      <xsd:documentation>Defines the parameters associated with a flight plan message type.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="aircraftIdentification" type="nce:aircraftIdType">
        <xsd:annotation>
          <xsd:documentation>Aircraft call sign/identification.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="facilityCID" type="computerCIDType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>This is the computer identification of the flight as assigned by the sending facility. It may only be present for proposed flights.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="aircraftSpecification" type="nce:flightAircraftSpecificationType">
        <xsd:annotation>
          <xsd:documentation>Aircraft specification.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="beaconCode" type="nce:beaconCodeType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Beacon code.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="airSpeed" type="nce:speedType">
        <xsd:annotation>
          <xsd:documentation>Air speed in knots.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="coordinationPoint" type="coordinationPointType">
        <xsd:annotation>
          <xsd:documentation>Coordination point.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="coordinationTime" type="nce:coordinationTimeType">
        <xsd:annotation>
          <xsd:documentation>Coordination time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:choice>
        <xsd:element name="assignedAltitude" type="fm2:filedAltitudeType">
          <xsd:annotation>
            <xsd:documentation>Assigned altitude.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="requestedAltitude" type="fm2:filedAltitudeType">
          <xsd:annotation>
            <xsd:documentation>Requested altitude.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:choice>
      <xsd:choice>
        <xsd:element name="filedRoute" type="nce:field10FormatType">
          <xsd:annotation>
            <xsd:documentation>Filed route.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="ambiguousRoute" type="ambiguousRouteType">
          <xsd:annotation>
            <xsd:documentation>Ambiguous route.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:choice>
      <xsd:element name="remarks" type="nce:flightPlanRemarks" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Remarks.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="fosRequestType">
    <xsd:annotation>
      <xsd:documentation>Defines the FOS request Type.</xsd:documentation>
    </xsd:annotation>
    <xsd:choice>
      <xsd:element name="TOS_MESSAGE" type="fcm:tosType">
        <xsd:annotation>
          <xsd:documentation>The trajectory option set for a specific flight (320)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TOS_REQ" type="fcm:fltIdType">
        <xsd:annotation>
          <xsd:documentation>Request for the current trajectory option set for the specified flight. (323)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TRAJ_ASSIGN_REQ" type="fcm:fltIdType">
        <xsd:annotation>
          <xsd:documentation>Request for the current trajectory assignment for a specific flight (339)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CTOP_SUB_REQ">
        <xsd:annotation>
          <xsd:documentation>Request to reassign existing flights to existing slots (350)</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:all>
            <xsd:element name="CTOP_NAME" type="fcm:ctopNameType">
              <xsd:annotation>
                <xsd:documentation>Name of the CTOP that controls the flights being substituted</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="CTOP_SHORT_NAME" type="fcm:ctopTmiIdType">
              <xsd:annotation>
                <xsd:documentation>System generated 6 char ID/name for the CTOP TMI.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="CTOP_SUB_MODE" type="fcm:subModeType">
              <xsd:annotation>
                <xsd:documentation>Specifies strict or flexible processing for the sub request</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="FLIGHT_LIST">
              <xsd:annotation>
                <xsd:documentation>Container for the flights being substituted</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="FLIGHT" minOccurs="2" maxOccurs="500">
                    <xsd:annotation>
                      <xsd:documentation>Container  for a single flight</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:all>
                        <xsd:element name="UNIQUE_FLT_ID_DATA" type="commonCompositeFlightIdType">
                          <xsd:annotation>
                            <xsd:documentation>Container tag added for grouping aircraft identification data with a common type</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="SLOT" type="fcm:fltSlotType">
                          <xsd:annotation>
                            <xsd:documentation>Requested slot</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                      </xsd:all>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="fosReplyDataType">
    <xsd:annotation>
      <xsd:documentation>Messages sent to the FOS user from TFMS along with additional information needed by the portal</xsd:documentation>
    </xsd:annotation>
    <xsd:choice>
      <xsd:element name="tosReplyData">
        <xsd:annotation>
          <xsd:documentation>Response to receipt of invalid or partially invalid CTOP TOS Message (322)</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:all>
            <xsd:element name="UNIQUE_FLT_ID_DATA" type="fcm:fltIdType">
              <xsd:annotation>
                <xsd:documentation>Container tag added for grouping aircraft identification data with a common type</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="TOS_SEQ_NO" type="fcm:tosSeqNbrType">
              <xsd:annotation>
                <xsd:documentation>Unique sequence number used to correlate trajectory update messages with TOSs.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="TRAJ_OPTION_LIST" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Container for the trajectory option list</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="TRAJ_OPTION" maxOccurs="100">
                    <xsd:annotation>
                      <xsd:documentation>Container  for a single trajectory option for this flight</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:all>
                        <xsd:element name="TRAJ_INDEX" type="fcm:trajIndexType">
                          <xsd:annotation>
                            <xsd:documentation>The trajectory index.</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="ERROR_LIST" type="fcm:errorListType">
                          <xsd:annotation>
                            <xsd:documentation>Associated error list.</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                      </xsd:all>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="tosResyncData" type="fcm:tosType">
        <xsd:annotation>
          <xsd:documentation>Response to valid CTOP TOS resynch Request(324)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ctopSubReplyData">
        <xsd:annotation>
          <xsd:documentation>Response to a valid CTOP Substitution Request (351)</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:all>
            <xsd:element name="CTOP_NAME" type="fcm:ctopNameType">
              <xsd:annotation>
                <xsd:documentation>CTOP name.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="CTOP_SHORT_NAME" type="fcm:ctopTmiIdType">
              <xsd:annotation>
                <xsd:documentation>System-generated unique identifier for the CTOP.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="FLIGHT_LIST">
              <xsd:annotation>
                <xsd:documentation>Container for all the flights that were substituted</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="FLIGHT" minOccurs="2" maxOccurs="unbounded">
                    <xsd:annotation>
                      <xsd:documentation>Container for a flight</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:all>
                        <xsd:element name="UNIQUE_FLT_ID_DATA" type="fcm:fltIdType">
                          <xsd:annotation>
                            <xsd:documentation>Container tag added for grouping aircraft identification data with a common type</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="ERTD" type="xsd:dateTime" minOccurs="0">
                          <xsd:annotation>
                            <xsd:documentation>Earliest date and time the flight can depart even if the constraint is lifted.</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="TOS_SEQ_NO" type="fcm:tosSeqNbrType" minOccurs="0">
                          <xsd:annotation>
                            <xsd:documentation>Current TOS sequence number for this flight. This tag is omitted if no TOS has been submitted for this flight</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="SLOT" type="fcm:fltSlotType" minOccurs="0">
                          <xsd:annotation>
                            <xsd:documentation>Requested slot</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="TRAJ_OPTION_LIST">
                          <xsd:annotation>
                            <xsd:documentation>Container for the trajectory option list.</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element name="TRAJ_OPTION" maxOccurs="unbounded">
                                <xsd:annotation>
                                  <xsd:documentation>Container for a single trajectory option for this flight.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                  <xsd:all>
                                    <xsd:element name="TRAJ_INDEX" type="fcm:trajIndexType" minOccurs="0">
                                      <xsd:annotation>
                                        <xsd:documentation>Index for the selected trajectory as provided in the TOS message.</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="ASSIGNED">
                                      <xsd:annotation>
                                        <xsd:documentation>Indicator of whether this is an assigned trajectory</xsd:documentation>
                                      </xsd:annotation>
                                      <xsd:simpleType>
                                        <xsd:restriction base="xsd:string">
                                          <xsd:enumeration value="TRUE"/>
                                          <xsd:enumeration value="FALSE"/>
                                        </xsd:restriction>
                                      </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="ADJUSTED_COST" type="fcm:trajAdjCostType">
                                      <xsd:annotation>
                                        <xsd:documentation>Adjusted cost the flight would incur if this were the assigned trajectory option.</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="REL_TRAJ_COST" type="fcm:trajRelCostType" minOccurs="0">
                                      <xsd:annotation>
                                        <xsd:documentation>Relative cost of one trajectory as compared to another for this flight expressed in minutes of delay</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="ROUTE" type="xsd:string" minOccurs="0">
                                      <xsd:annotation>
                                        <xsd:documentation>Route of flight assigned by the CTOP in ICAO format. Required when ASSIGNED=TRUE</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="ALT" type="fcm:trajAltType" minOccurs="0">
                                      <xsd:annotation>
                                        <xsd:documentation>Cruising altitude assigned by the CTOP in hundreds of feet. Required when ASSIGNED=TRUE</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="SPEED" type="fcm:trajSpdType" minOccurs="0">
                                      <xsd:annotation>
                                        <xsd:documentation> Cruising speed assigned by the CTOP in knots. Required when ASSIGNED=TRUE</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="CTD" type="xsd:dateTime" minOccurs="0">
                                      <xsd:annotation>
                                        <xsd:documentation>Controlled date and time of departure for the flight assigned by the CTOP. If not present, the flight can depart at any time on the assigned route.</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="FCA_LIST">
                                      <xsd:annotation>
                                        <xsd:documentation>Container for the list of FCAs that are part of this CTOP and are intersected by this trajectory option.</xsd:documentation>
                                      </xsd:annotation>
                                      <xsd:complexType>
                                        <xsd:sequence>
                                          <xsd:element name="FCA" minOccurs="0" maxOccurs="unbounded">
                                            <xsd:annotation>
                                              <xsd:documentation>Container for an FCA</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                              <xsd:all>
                                                <xsd:element name="FCA_ID" type="fcm:fcaIdType">
                                                  <xsd:annotation>
                                                    <xsd:documentation>System generated unique identifier for this FCA</xsd:documentation>
                                                  </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="FCA_EARLIEST_ENTRY" type="xsd:dateTime">
                                                  <xsd:annotation>
                                                    <xsd:documentation>Earliest time the flight could enter this FCA on this trajectory option.</xsd:documentation>
                                                  </xsd:annotation>
                                                </xsd:element>
                                              </xsd:all>
                                            </xsd:complexType>
                                          </xsd:element>
                                        </xsd:sequence>
                                      </xsd:complexType>
                                    </xsd:element>
                                  </xsd:all>
                                </xsd:complexType>
                              </xsd:element>
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                      </xsd:all>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="trajAssignResyncData" type="fcm:ctopTrajAssignType"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="capacityValuesType">
    <xsd:annotation>
      <xsd:documentation>This complex type defines the capacity values for thresholds and general aviation estimates. For each capacity, there is a nominal and today value.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="nominalValue" type="xsd:nonNegativeInteger">
        <xsd:annotation>
          <xsd:documentation>This element contains the nominal value for a capacity.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="todayValue" type="xsd:nonNegativeInteger">
        <xsd:annotation>
          <xsd:documentation>This element contains the today value for a capacity.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="commonCompositeFlightIdType">
    <xsd:annotation>
      <xsd:documentation>Defines parameters required to identify a specific flight.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="callSign" type="nce:aircraftIdType">
        <xsd:annotation>
          <xsd:documentation>Call sign/identifier.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="departureAirport" type="nce:airportIdType">
        <xsd:annotation>
          <xsd:documentation>Departure airport.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="arrivalAirport" type="nce:airportIdType">
        <xsd:annotation>
          <xsd:documentation>Arrival airport.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="departureDateTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Departure date and time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="fcaIdType">
    <xsd:annotation>
      <xsd:documentation>Defines the FEA/FCA Identifier.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="FCA_ID" type="fm2:uniqueFcaIdType">
        <xsd:annotation>
          <xsd:documentation>System-generated unique identifier of the FCA.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FCA_START_TIME" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Start time of FCA.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FCA_END_TIME" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>End time of FCA.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FCA_LOOK_AHEAD_RANGE" type="xsd:int">
        <xsd:annotation>
          <xsd:documentation>Look ahead range in minutes.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MODEL_STATISTICS" type="modelStatistics" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>An optional set of statistics provided when modeling a CTOP.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="flightCancelType">
    <xsd:annotation>
      <xsd:documentation>An individual flight in the TFMS database will be cancelled by using a Cancel message.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="commonCompositeFlightId" type="commonCompositeFlightIdType">
        <xsd:annotation>
          <xsd:documentation>Flight identification.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="slotHoldFlag" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Slot hold flag indicator.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:simpleContent>
            <xsd:extension base="xsd:string"/>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="flightCreateType">
    <xsd:annotation>
      <xsd:documentation>An individual flight is added/updated to the TFMS database using a Flight Create (FC) message</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="commonCompositeFlightId" type="commonCompositeFlightIdType">
        <xsd:annotation>
          <xsd:documentation>Flight identification.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="originalCallSign" type="nce:aircraftIdType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Original call sign.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="originalDepartureDateTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Original departure time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="runwayDepartureTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Runway departure time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="runwayArrivalTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Runway arrival time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="gateDepartureTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Gate departure time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="gateArrivalTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Gate arrival time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="earliestRunwayDepartureTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Earliest Runway departure time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="earliestRunwayArrivalTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Earliest runway arrival time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="aircraftSpecification" type="nce:flightAircraftSpecificationType">
        <xsd:annotation>
          <xsd:documentation>Aircraft specification.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="remark" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Remarks.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:pattern value="DVRSN|DIVERT"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="terminalData" type="airlineSurfaceDataType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>TFDM Flight Data.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="flightModifyType">
    <xsd:annotation>
      <xsd:documentation>A Flight Modify (FM) message may be used to modify any field associated with a flight.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="commonCompositeFlightId" type="commonCompositeFlightIdType">
        <xsd:annotation>
          <xsd:documentation>Flight Identification.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="modifiedCallSign" type="nce:aircraftIdType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Updated aircraft call sign/identification.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="modifiedAircraftSpecification" type="nce:flightAircraftSpecificationType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Updated aircraft specification.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="modifiedDepartureAirport" type="nce:airportIdType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Updated departure airport.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="modifiedArrivalAirport" type="nce:airportIdType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Updated arrival airport.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="modifiedDepartureDateTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Updated departure time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="slotHoldFlag" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Slot hold indicator.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:pattern value="[H]|[R]"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="actualRunwayDepartureTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Actual runway departure time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="predictedRunwayDepartureTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Predicted runway departure time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="predictedRunwayArrivalTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Predicted runway arrival time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="actualGateDepartureTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Actual runway departure time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="predictedGateDepartureTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Predicted gate departure time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="predictedGateArrivalTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Predicted gate arrival time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="actualGateArrivalTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Actual gate arrival time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="actualRunwayArrivalTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Actual runway departure time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="earliestRunwayDepartureTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Earliest runway departure time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="earliestRunwayArrivalTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Earliest runway arrival time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="remark" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Remarks.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:pattern value="DVRSN|DIVERT"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="terminalData" type="airlineSurfaceDataType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>TFDM Flight Data.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ssFlightModifyType">
    <xsd:annotation>
      <xsd:documentation>A Flight Modify (FM) message may be used to modify any field associated with a flight. The FM message that is contained in a SS packet has different restrictions than an FM message from a FD packet.  In a SS packet, the departure or arrival airport is not allowed to be modified.  Also, the Controlled Departure Time, the Controlled Arrival Time, and the Assigned Arrival Slot is allowed to be modified in a SS packet and all must be grouped together.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="commonCompositeFlightId" type="commonCompositeFlightIdType">
        <xsd:annotation>
          <xsd:documentation>Flight identification.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="modifiedCallSign" type="nce:aircraftIdType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Updated call sign.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="modifiedAircraftSpecification" type="nce:flightAircraftSpecificationType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Updated Aircraft specification.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="modifiedDepartureAirport" type="nce:airportIdType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Updated departure airport.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="modifiedArrivalAirport" type="nce:airportIdType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Updated arrival airport.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="modifiedDepartureDateTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Updated departure time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="slotHoldFlag" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Slot hold indicator.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:pattern value="[H]|[R]"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="originalFlightIdentification" type="nce:aircraftIdType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Original flight identification.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="originalUTCDepartureDateTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Original departure date and time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="predictedGateDepartureTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Predicted gate departure time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="predictedGateArrivalTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Predicted gate arrival time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="predictedRunwayDepartureTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Predicted runway departure time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="predictedRunwayArrivalTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Predicted runway arrival time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="actualGateDepartureTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Actual gate departure time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="actualGateArrivalTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Actual gate arrival time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="earliestRunwayDepartureTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Earliest runway departure time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="earliestRunwayArrivalTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Earliest runway arrival time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="actualRunwayDepartureTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Actual runway departure time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="actualRunwayArrivalTime" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Actual runway arrival time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="remark" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Remarks.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:pattern value="DVRSN|DIVERT"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="controlledDepartureTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Controlled departure time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="controlledArrivalTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Controlled arrival time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="assignedArrivalSlot" type="arrivalSlotType">
        <xsd:annotation>
          <xsd:documentation>Assigned arrival slot.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="slotCreditSubstitutionType">
    <xsd:annotation>
      <xsd:documentation>This message from a client to the server sends simplified subs or slot credit sub messages from the airline to the server. The data buffer contains the packet header followed by a variable number of messages terminated by line feeds. The short data field contains a sequence number, which is simply an integer incremented by one for each packet sent; the range of the sequence numbers is 1 to 9999.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="commonCompositeFlightId" type="commonCompositeFlightIdType">
        <xsd:annotation>
          <xsd:documentation>Flight identification.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="earliestDepartureTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Earliest departure time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="latestArrivalTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Latest arrival time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="arrivalSlot" type="arrivalSlotType">
        <xsd:annotation>
          <xsd:documentation>Arrival slot.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="slotCreateType">
    <xsd:annotation>
      <xsd:documentation>The Slot Create (SC) message allows an airline to manage this scenario. Using slot create, the airline can voluntarily control FLIGHT2, creating an arrival slot for it at its normally planned arrival time. Once the slot is created, the airline can swap the two flights using the usual mechanisms.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="commonCompositeFlightId" type="commonCompositeFlightIdType">
        <xsd:annotation>
          <xsd:documentation>Flight identification.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="controlledDepartureTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Controlled departure time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="controlledArrivalTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Controlled arrival time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="assignedArrivalSlot" type="arrivalSlotType">
        <xsd:annotation>
          <xsd:documentation>Assigned arrival slot.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="allSlotsType">
    <xsd:annotation>
      <xsd:documentation>Release or hold all slots.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="msgType">
        <xsd:annotation>
          <xsd:documentation>Request type: Release or hold..</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="RELEASE"/>
            <xsd:enumeration value="HOLD"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="controlElement" type="fce:controlElementNameType">
        <xsd:annotation>
          <xsd:documentation>Control element.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="oceanicNonPosRclPositionReportType">
    <xsd:annotation>
      <xsd:documentation>Defines an non PS/RCL oceanic reports.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="aircraftIdentifier" type="nce:aircraftIdType">
        <xsd:annotation>
          <xsd:documentation>2 to 7 characters in length with first character being an uppercase letter.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:sequence minOccurs="1" maxOccurs="12">
        <xsd:element name="oceanicDataBlock" type="oceanicDataBlockType">
          <xsd:annotation>
            <xsd:documentation>Oceanic data report.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="oceanicPosRclPositionReportType">
    <xsd:annotation>
      <xsd:documentation>Defines an oceanic POS/RCL position report.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="aircraftIdentifier" type="nce:aircraftIdType">
        <xsd:annotation>
          <xsd:documentation>2 to 7 characters in length with first character being an uppercase letter.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:sequence maxOccurs="3">
        <xsd:element name="oceanicDataBlock" type="oceanicDataBlockType">
          <xsd:annotation>
            <xsd:documentation>Oceanic data report.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="oceanicReportMessageType">
    <xsd:annotation>
      <xsd:documentation>Defines an oceanic report messages.</xsd:documentation>
    </xsd:annotation>
    <xsd:choice minOccurs="1">
      <xsd:element name="oceanicPosRclPositionReport" type="oceanicPosRclPositionReportType">
        <xsd:annotation>
          <xsd:documentation>Oceanic position report.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="oceanicNonPosRclPositionReport" type="oceanicNonPosRclPositionReportType">
        <xsd:annotation>
          <xsd:documentation>Oceanic other reports.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="oceanicDataBlockType">
    <xsd:annotation>
      <xsd:documentation>Oceanic data report.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="position" type="fm2:filedFixType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Fix represented as latitude/longitude pair or fix name.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="eta" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Estimated time of arrival (ETA) includes date and time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="altitude" type="fm2:altitudeType" minOccurs="0"></xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="arrivalSlotType">
    <xsd:annotation>
      <xsd:documentation>Defines an arrival slot.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="airport" type="fce:controlElementNameType">
        <xsd:annotation>
          <xsd:documentation>Airport name.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="slotTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>The slot time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="uniqueLetter" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>A unique letter.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="rriaRerouteCtrHeadType">
    <xsd:annotation>
      <xsd:documentation>Model reroute header type.</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:annotation>
        <xsd:documentation>ALL to require each item (unless optional) in ANY ORDER and at most one occurence of each.</xsd:documentation>
      </xsd:annotation>
      <xsd:element name="ID" type="rriaIdType">
        <xsd:annotation>
          <xsd:documentation>System-generated reroute identifier. Up to 64 characters.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="originalRerouteId" type="rriaIdType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>When a reroute is copied, the original reroute ID is saved with the reroute definition.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="NAME" type="rerouteNameType">
        <xsd:annotation>
          <xsd:documentation>Reroute name. Up to 64 characters.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DOMAIN" type="nce:tmiVisibilityDomainType">
        <xsd:annotation>
          <xsd:documentation>Possible values are PUBLIC,  SHARED, LOCAL, or PRIVATE.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ORIGINAL_CREATETIME" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Date and time the reroute was first created.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="LASTUPDATE" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Date and time reroute was last updated</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="STATUS">
        <xsd:annotation>
          <xsd:documentation>One of ACTIVE, PLANNED.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="ACTIVE"/>
            <xsd:enumeration value="PLANNED"/>
            <xsd:enumeration value="EXPIRED"/>
            <xsd:enumeration value="DELETE"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="SHOW">
        <xsd:annotation>
          <xsd:documentation>Four digits (each 0 or 1) specifying whether to draw fixes, NAVAIDs, reroute label, and/or the reroute by default.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:pattern value="[01]{4}"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="COLOR_ID">
        <xsd:annotation>
          <xsd:documentation>Color index, value 0 - 35</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:int">
            <xsd:minInclusive value="0"/>
            <xsd:maxInclusive value="35"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="REROUTE_TIMETYPE">
        <xsd:annotation>
          <xsd:documentation>One of ETA, ETD, FCA, TAC indicating the method used to select flights involved in the reroute.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="ETA"/>
            <xsd:enumeration value="ETD"/>
            <xsd:enumeration value="FCA"/>
            <xsd:enumeration value="TAC"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="REROUTE_STARTTIME" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Reroute start time</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="REROUTE_ENDTIME" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Reroute end time</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FCA_NAME" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Name of FCA if reroute is based on FCA. Present if REROUTE_TIME_TYPE is FCA. Up to 30 characters.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:pattern value="[A-Za-z0-9_\-]{0,30}"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="AIRBORNE">
        <xsd:annotation>
          <xsd:documentation>One of AIRBORNE, NOT_AIRBORNE, ALL_FLIGHTS, indicating which flights are included in the reroute by flight status.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="AIRBORNE"/>
            <xsd:enumeration value="NOT_AIRBORNE"/>
            <xsd:enumeration value="ALL_FLIGHTS"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="SHARED_SITES" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Space-separated list of TFMS sites with which the Reroute was shared.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction>
            <xsd:simpleType>
              <xsd:list itemType="fce:siteType"/>
            </xsd:simpleType>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="canceledReroutes" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List of the reroute IDs of the existing reroutes that are being replaced by this reroute. (Used only for reroute modeling purposes.)</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="rerouteId" type="rriaIdType" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>ID's of reroutes to be cancelled before the model</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="tmiId" type="fce:tmiIdType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>TMD ID of the rerouting initiative</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="monitorReroute" type="xsd:boolean" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>One of true or false. True indicates that the reroute is to be monitored by the TFMS Reroute Monitor process.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="applyProtectedSegments" type="xsd:boolean" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>One of true or false. True indicates that the Protected Segments are to be included in every assigned route and applied in the conformance determination by the TFMS Reroute Monitor process</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="cancelDroppedFlights" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>One of ALL or NOT_AIRBORNE. When the reroute is edited and the value is NOT_AIRBORNE, cancellation messages will be sent to ERAM for just the rerouted flights that are dropped from the reroute and are in a pre-departure status. Otherwise the cancellation message will be sent for all dropped flights. These messages will remove the TMI ID and protected segments from the ERAM fight records. Default is ALL.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="ALL"/>
            <xsd:enumeration value="NOT_AIRBORNE"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="tmiIdHistory" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>When a reroute is edited a new TMI ID may be assigned to the reroute. This field provides a comma separated list of all prior TMI IDs that have been associated with the reroute in reverse chorological order.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:pattern value="[A-Z0-9]{1,10}(,[A-Z0-9]{1,10})*"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="rriaRerouteCtrMitType">
    <xsd:annotation>
      <xsd:documentation>Reroute MIT type.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="MIT_ID">
        <xsd:annotation>
          <xsd:documentation>MIT identifier.  Must be unique within the scope of this reroute.  Up to 30 characters.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="30"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="includeMIT" type="xsd:boolean">
        <xsd:annotation>
          <xsd:documentation>Include MIT indicator.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SPACING">
        <xsd:annotation>
          <xsd:documentation>The spacing to be applied to flights in this MIT, in miles. For an FEA/FCA based MIT, this value can be overridden by the spacing applied to flights in a secondary filter.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:pattern value="EXCLUDE|\d{1,3}"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="CTR_MIT_FILTER_LIST" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>MIT filters.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="CTR_MIT_FILTER" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>MIT filter.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:choice>
                    <xsd:element name="MIT_DESTINATION_FILTER_ID" type="rriaIdType">
                      <xsd:annotation>
                        <xsd:documentation>Present only if the MIT filter is based on a destination airport.  For an MIT based on a crossing segment or fix, the ID will be the destination airport name.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="MIT_FEA_FILTER_ID" type="rriaIdType">
                      <xsd:annotation>
                        <xsd:documentation>Present only if the MIT filter is based on a FXA secondary filter.  System generated unique ID of a secondary filter associated with the FEA/FCA.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                  </xsd:choice>
                  <xsd:element name="MIT_FILTER_SPACING">
                    <xsd:annotation>
                      <xsd:documentation>The spacing to be applied to flights in this secondary filter in miles.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:string">
                        <xsd:pattern value="EXCLUDE|\d{1,3}"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="APPLY_TO_FLIGHTS">
        <xsd:annotation>
          <xsd:documentation>One of ALL, REROUTED, NON-REROUTED. Indicates that the MIT restriction will be applied to all flights that match the restriction criteria or only the rerouted or non-rerouted flights.  ALL and NON-REROUTED are allowed only for FEA/FCA-based MIT.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="ALL"/>
            <xsd:enumeration value="REROUTED"/>
            <xsd:enumeration value="NON-REROUTED"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="MIT_TIME_TYPE">
        <xsd:annotation>
          <xsd:documentation>One of MANUAL, AUTO, or FEA_TIMES.  MANUAL means the user has set the start and end time of the MIT manually.  AUTO means that TFMS will compute a start and end time that ensures that all flights in the reroute are subject to the MIT. FEA_TIMES means that TFMS will set the MIT start and end times to the FEA/FCA start and end times.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="MANUAL"/>
            <xsd:enumeration value="AUTO"/>
            <xsd:enumeration value="FEA_TIMES"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="MIT_STARTTIME" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Start time for the MIT restriction (yyyymmddhhmmss).  MIT will be applied to any flight that is modeled to cross the MIT restriction point between the start time and the end time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MIT_ENDTIME" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>End time for the MIT restriction (yyyymmddhhmmss).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:choice>
        <xsd:element name="MIT_CROSSING_SEGMENT">
          <xsd:annotation>
            <xsd:documentation>The points of a multi-segment crossing line drawn on the TSD and used to select the reroute segments affected by the MIT restriction. If present, the crossing segment data is only used by the TSD to draw the crossing line. It is not used by the backend to determine the segments that are part of the MIT. Each point is a comma-delimited latitude and longitude in hundredths of degrees (i.e., degrees times 100). Latitudes are assumed to be North and longitudes West. Points are separated by spaces. (This is same data structure as the one used for Line FCAs.)</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:pattern value="(\d{3,5},\d{3,5}[ ]*)+"/>
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
        <xsd:element name="MIT_FIX" type="nce:fixNameType">
          <xsd:annotation>
            <xsd:documentation>A named fix where the MIT is to be applied.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="MIT_FEA" type="rriaIdType">
          <xsd:annotation>
            <xsd:documentation>The ID of an FCA/FEA that will be used to specify the flights that will be subject to the MIT restriction.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:choice>
      <xsd:element name="CTR_MIT_SEGMENT_LIST" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Container for the reroute segments affected by this MIT. Will not be present if the MIT restriction is based on an FEA/FCA.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="CTR_MIT_SEGMENT" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>MIT segment.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="SEGMENT_ID" type="rriaRerouteSegmentIdType">
                    <xsd:annotation>
                      <xsd:documentation>The ID of a reroute segment to which the MIT will be applied.  If a crossing segment was specified, TSD identifies each included reroute segment that the line crosses.  If a fix was specified, TSD identifies each included reroute segment that contains the fix.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="MIT_LATLON" type="rriaLatLongType" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>The location where the crossing segment crosses the reroute segment.  Comma-separated latitude and longitude in hundredths of degrees. This is the location where the MIT will be applied to flights on this segment.  This tag is present only if a crossing segment was specified for the MIT.  If a fix was specified, then the MIT will be applied at the fix, in the same location for all segments.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="CTR_MIT_FEA_AS_ONE_FILTER_LIST" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Container for MIT FEA.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:choice maxOccurs="unbounded">
            <xsd:element name="MIT_FEA_AS_ONE_FILTER_ID" type="rriaIdType">
              <xsd:annotation>
                <xsd:documentation>MIT FEA filter identifier.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:choice>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="rriaRerouteCtrSeg">
    <xsd:annotation>
      <xsd:documentation>Container for reroute segment.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="INCLUDE">
        <xsd:annotation>
          <xsd:documentation>Indicates whether this segment is included  in the reroute or excluded.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="INCLUDE"/>
            <xsd:enumeration value="EXCLUDE"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="SEGMENT_VALID">
        <xsd:annotation>
          <xsd:documentation>"Y" if this reroute segment is valid. "N" otherwise.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Y"/>
            <xsd:enumeration value="N"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="ORIGIN" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List of origin airports/centers, separated by spaces. Up to 128 characters.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:pattern value="[a-zA-Z0-9 ]{0,128}"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="DESTINATION" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List of destination airports/centers, separated by spaces. Up to 128 characters.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:pattern value="[a-zA-Z0-9 ]{0,128}"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="FLTR_SUMMARY" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>System-generated text summarizing any filter conditions defined for this reroute segment. Up to 100 characters.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:pattern value=".{0,100}"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="ROUTE_STR">
        <xsd:annotation>
          <xsd:documentation>Route string, up to 256 characters. Note that route elements are separated by spaces.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="256"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="FULL_ROUTE">
        <xsd:annotation>
          <xsd:documentation>One of FULL, ORIG, DEST.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="FULL"/>
            <xsd:enumeration value="ORIG"/>
            <xsd:enumeration value="DEST"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="WAYPOINT" maxOccurs="unbounded" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Gives LAT, LONG, name, and type (separated by commas, up to 32 characters total) for each waypoint on the route segment. Each waypoint is in a separate tag. Type is one of F=fix, N=NAVAID, W=waypoint, C=Center Entry. For an un-named waypoint, name is '?'.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="32"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="ROUTE_TYPE">
        <xsd:annotation>
          <xsd:documentation>User-specified route-segment type. One of NONE, CDR RTE, RERTE, UNKN RTE, UPT RTE</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="NONE"/>
            <xsd:enumeration value="CDR RTE"/>
            <xsd:enumeration value="RERTE"/>
            <xsd:enumeration value="UNKN RTE"/>
            <xsd:enumeration value="UPT RTE"/>
            <xsd:enumeration value="EXCEPTED"/>
            <xsd:enumeration value="ASSIGNED:"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="SEG_SOURCENAME" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>If this route segment was retrieved from a playbook, this is the name of the play. Up to 64 characters.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="64"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="SEG_ID" type="rriaRerouteSegmentIdType">
        <xsd:annotation>
          <xsd:documentation>The route grid row in which this segment appears. Rows in the upper (origin) grid begin with 'O' and rows in the lower (destination) grid begin with 'D'. Within each grid, rows are sequentially numbered beginning with 0. That is, Origin segments rows are O0, O1 etc., while Destination segment rows are D0, D1 etc. Up to 6 characters.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="protectedSegment" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>All or a portion of the route string, up to 1000 characters, that is designated as the protected portion.  Note that route elements are separated by spaces. The protected segment must begin and end with a named fix or airport that falls on the route segment.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="1000"/>
            <xsd:pattern value="([A-Z0-9]{2,} ?)+"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="SEG_REMARKS" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>User-entered remark. Up to 512 characters.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="512"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="CTR_FLTR" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>If present, at least one sub element must be present as well.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:annotation>
              <xsd:documentation>At least one is required, but any number may exist.</xsd:documentation>
            </xsd:annotation>
            <xsd:element name="NOT_FROM" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>List of excluded departure airports separated by spaces. Up to 128 characters.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="128"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="NOT_TO" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>List of excluded destination airports separated by spaces. Allowed only if the DESTINATION for this segment contains a center or UNKN. Up to 128 characters.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="128"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="THRU" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>List of centers separated by spaces. If present, flight must traverse at least one of the listed centers. Up to 128 characters.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="128"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="AIRWAY" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>List of airway names separated by spaces. If present, flight must fly on at least one of the listed airways. Up to 128 characters.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="128"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="USE_FIX" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>List of fix names separated by spaces. If present, flight must cross at least one of the listed fixes. Up to 128 characters.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="128"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="AC_TYPE" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>List of aircraft types separated by spaces. If present, flight must be one of the listed types. Up to 64 characters.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="64"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="FLIGHTLVLLOW" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Minimum flight level (cruising altitude), 0-600. Default = 0.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:int">
                  <xsd:minInclusive value="0"/>
                  <xsd:maxInclusive value="600"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="FLIGHTLVLHIGH" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Maximum flight level (cruising altitude), 0-600. Default = 600.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:int">
                  <xsd:maxInclusive value="600"/>
                  <xsd:minInclusive value="0"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="AIR_CAT">
              <xsd:annotation>
                <xsd:documentation>Aircraft category. One or more of J, P, T, separated by spaces. If present, flight must be one of the listed categories.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:pattern value="[JPT ]+"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="WEIGHT_CLASS" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Aircraft weight class. One or more of L, H, S, separated by spaces. If present, flight must be one of the listed weight classes.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:pattern value="[LHS ]+"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="USER_CAT">
              <xsd:annotation>
                <xsd:documentation>User type. One or more of T, F, C, G, M, separated by spaces. G=GA T=AirTaxi F=Cargo C=Commercial M=Military. If present, flight must be one of the listed user categories.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:pattern value="[TFCGM ]+"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="RVSM" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>One of ALL, COMPLIANT, NON_COMPLIANT. Indicates which flights to include by their RVSM status. Default is ALL. Note there is a CTR_FLIGHT tag with the same name but a different value set.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="ALL"/>
                  <xsd:enumeration value="COMPLIANT"/>
                  <xsd:enumeration value="NON-COMPLIANT"/>
                  <xsd:enumeration value="NON_COMPLIANT"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="FLTR_TIMETYPE" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>One of DEP, ARR, indicating that flights are filtered by departure time or arrival time. FLTR_TIMETYPE, FLTR_STARTTIME and FLTR_ENDTIME tags are all present or all absent.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="DEP"/>
                  <xsd:enumeration value="ARR"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="FLTR_STARTTIME" type="xsd:dateTime" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Filter start date and time.  If present, only flights that depart or arrive (depending on FLTR_TIMETYPE) within the specified start and end time are included.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="FLTR_ENDTIME" type="xsd:dateTime" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Filter end date and time.  If present, only flights that depart or arrive (depending on FLTR_TIMETYPE) within the specified start and end time are included.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="rriaRerouteSegmentIdType">
    <xsd:annotation>
      <xsd:documentation>The route grid row in which this segment appears. Rows in the upper (origin) grid begin with 'O' and rows in the lower (destination) grid begin with 'D'. Within each grid, rows are sequentially numbered beginning with 0. That is, Origin segments rows are O0, O1 etc., while Destination segment rows are D0, D1 etc. Up to 6 characters.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="(C|(O|D)\d{1,5})"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="rriaCombinedSegmentIdType">
    <xsd:annotation>
      <xsd:documentation>For a full route, the route grid row in which this segment appears. For a split route, a space-separated pair of route grid rows, first for the origin row, and second for the destination row.  Rows in the upper (origin) grid begin with "O", and rows in the lower (destination) grid begin with "D".  Within each grid, rows are sequentially numbered beginning with 0.  That is, Origin segments rows are O0, O1 etc., while Destination segment rows are D0, D1 etc. Up to 6 characters.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="(C|(O)\d{1,5}(( D)\d{1,5})?)"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="rriaAlertStatusType">
    <xsd:annotation>
      <xsd:documentation>Comma separated R's Y's, and G's representing alert status over time.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="(G,|Y,|R,)*[GRY]"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="rriaLatLongType">
    <xsd:annotation>
      <xsd:documentation>Format used to send Latitude/Longitude pairs in RRIA messages. Both value in the pair should be in the "Hundred" format which means: standard format(rounded to the hundredth decimal place) * 100</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="-?(9000|[0-8]?(\d){1,3}),[ ]?-?(18000|\d{1,4}|[0-1][0-7]\d{3})"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="rriaIdType">
    <xsd:annotation>
      <xsd:documentation>Format used to define various IDs across RRIA.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[a-zA-Z0-9\.]{0,64}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="rriaCountsPositiveType">
    <xsd:annotation>
      <xsd:documentation>Comma separated positive integers representing flight counts over time.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="(\d)+(,\d+)*"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="ctrMitRestrictionType">
    <xsd:annotation>
      <xsd:documentation>MIT restriction type.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="mitId">
        <xsd:annotation>
          <xsd:documentation>MIT ID</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="30"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="mitFeaFilterId" type="rriaIdType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>FEA filter ID present if separate spacing has been specified for flights that match an FEA secondary filter.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="mitDestinationFilterId" type="nce:airportIdType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Airport name, present if separate spacing has been specified for flights that are going to a specified destination airport.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="mitAirborneDelay" type="xsd:int" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Airborne delay resulting from this MIT. Measured in minutes.  Present only if there is airborne MIT delay for this flight due to this MIT.  At most one of MIT_AIRBORNE_DELAY and MIT_GROUND_DELAY can be present for a given MIT and flight.  If there is no delay for this flight due to this MIT, then neither field is present.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="mitGroundDelay" type="xsd:int" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Ground delay resulting from this MIT. Measured In minutes. .  Present only if there is MIT ground delay for this flight due to this MIT. At most one of MIT_AIRBORNE_DELAY and MIT_GROUND_DELAY can be present for a given MIT and flight.  If there is no delay for this flight due to this MIT, then neither field is present.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="mitCrossingTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Crossing time when flight reaches this MIT point, without any MIT delay for this reroute.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="mitCrossingTimeWithMit" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Crossing time when flight reaches this MIT after all MIT delay associated with this reroute has been applied.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ctrSectorDataCommon">
    <xsd:annotation>
      <xsd:documentation>Sector common parameters type.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="todayMapOn" type="rriaCountsPositiveType">
        <xsd:annotation>
          <xsd:documentation>Current Monitor Alert Parameter ON value for each interval.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="todayMapOff" type="rriaCountsPositiveType">
        <xsd:annotation>
          <xsd:documentation>Current Monitor Alert Parameter OFF value for each interval.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="alertStatusBefore" type="rriaAlertStatusType">
        <xsd:annotation>
          <xsd:documentation>RYG alert status of the sector for each interval without the reroute</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="alertStatusAfter" type="rriaAlertStatusType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>RYG alert status of the sector for each interval with the reroute. Only present for sectors that are impacted by the reroute.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="alertStatusAfterMit" type="rriaAlertStatusType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>RYG alert status of the sector for each interval after applying MIT restrictions. Only present for sectors that are impacted by the MIT restrictions.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="peakAllAfter" type="rriaCountsPositiveType">
        <xsd:annotation>
          <xsd:documentation>Peak one-minute count in the sector of all (currently active and proposed) traffic for each interval with the reroute</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="peakActiveAfter" type="rriaCountsPositiveType">
        <xsd:annotation>
          <xsd:documentation>Peak one-minute count in the sector of the currently active traffic for each interval with the reroute</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="peakAllDelta" type="rriaCountsDeltaType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Difference in the peak one-minute count in the sector of all traffic for each interval with and without the reroute. Positive delta indicates an increase in the count with the reroute. Only present for sectors that are impacted by the reroute.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="peakActiveDelta" type="rriaCountsDeltaType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Difference in the peak one-minute count in the sector of the currently active traffic for each interval with and without the reroute. Positive delta indicates an increase in the count with the reroute. Only present for sectors that are impacted by the reroute.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="peakAllAfterMit" type="rriaCountsPositiveType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Peak one-minute count in the sector of all (currently active and proposed) traffic for each interval with the MIT restrictions. Only present for sectors that are impacted by the MIT restrictions.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="peackActiveAfterMit" type="rriaCountsPositiveType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Peak one-minute count in the sector of the currently active traffic for each interval with the MIT restrictions. Only present for sectors that are impacted by the MIT restrictions</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="peakAllDeltaMit" type="rriaCountsDeltaType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Difference in the peak one-minute count in the sector of all traffic for each interval with and without the MIT restrictions. Positive delta indicates an increase in the count with the MIT restrictions. Only present for sectors that are impacted by the MIT restrictions.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="peakActiveDeltaMit" type="rriaCountsDeltaType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Difference in the peak one-minute count in the sector of the currently active traffic for each interval with and without the MIT restrictions. Positive delta indicates an increase in the count with the MIT restrictions. Only present for sectors that are impacted by the MIT restrictions.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="rriaCountsDeltaType">
    <xsd:annotation>
      <xsd:documentation>Comma separated positive integers representing flight count differences over time.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="(-)?(\d)+(,-?\d+)*"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="rriaAdvNumberType">
    <xsd:restriction base="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Advisory number. Up to 3 characters. --- if no advisory number.</xsd:documentation>
      </xsd:annotation>
      <xsd:pattern value="\-\-\-|\d{3}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="rriaModeledReroutesType">
    <xsd:sequence>
      <xsd:element name="ctrReroute" maxOccurs="unbounded">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="rrId" type="rriaIdType"/>
            <xsd:element name="rrName" type="rerouteNameType"/>
            <xsd:element name="rrAdvisoryNumber" minOccurs="0" type="rriaAdvNumberType"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="rriaFlightsDataType">
    <xsd:annotation>
      <xsd:documentation>Reroute model flight data type.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ctrHead">
        <xsd:annotation>
          <xsd:documentation>Container for the header data.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="rerouteId" type="rriaIdType">
              <xsd:annotation>
                <xsd:documentation>System-generated reroute identifier. Up to 64 characters.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="rerouteName" type="rerouteNameType">
              <xsd:annotation>
                <xsd:documentation>Reroute name. Up to 64 characters.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="rerouteTimeType">
              <xsd:annotation>
                <xsd:documentation>One of ETA, ETD, FCA, indicating the method used to select flights involved in the reroute.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="ETA"/>
                  <xsd:enumeration value="ETD"/>
                  <xsd:enumeration value="FCA"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="fcaName" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Name of FEA/FCA on which the reroute is based. Only present if REROUTE_TIMETYPE is FCA</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:pattern value="\w{0,30}"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="modelTime" type="xsd:dateTime">
              <xsd:annotation>
                <xsd:documentation>Time when the modeling was requested.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="ctrModeledReroutes" minOccurs="0" type="rriaModeledReroutesType"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ctrFlightList">
        <xsd:annotation>
          <xsd:documentation>Container for the flight list.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="ctrFlight" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Container for the data for a flight</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="includeFlight" type="xsd:boolean">
                    <xsd:annotation>
                      <xsd:documentation>Indicates whether this flight is included in the reroute. Set to INCLUDE if a flight has at least one CTR_ASSIGNED_RTE for which both INCLUDE_RTE and INCLUDE_SEGMENT are INCLUDE.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="otherRrIds" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>List of any other public required reroute(s) that include this flight. Present only if the flight is included in at least one other public required reroute.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="rerouteId" type="rerouteIdType" maxOccurs="unbounded">
                          <xsd:annotation>
                            <xsd:documentation>System-generated reroute identifier. Up to 64 characters.</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="acid" type="nce:aircraftIdType">
                    <xsd:annotation>
                      <xsd:documentation>Host Aircraft ID. This is the aircraft ID that TSD displays.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="zacid" minOccurs="0" type="nce:aircraftIdType">
                    <xsd:annotation>
                      <xsd:documentation>Stripped Aircraft ID - leading zeros removed</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="rvsm" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>One of NC, -. NC = RVSM non-conformant, - = RVSM conformant. Note there is a CTR_FILTER tag with the same name but a different value set.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="NC"/>
                        <xsd:enumeration value="-"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="impactedByReroute" type="xsd:boolean">
                    <xsd:annotation>
                      <xsd:documentation>Flag indicating whether the flight is impacted by the reroute.  Will be N only if MIT is being applied to non-rerouted flights, and this flight is in the MIT FEA/FCA but is not in the reroute.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="impactedByMIT" type="xsd:boolean">
                    <xsd:annotation>
                      <xsd:documentation>Flag indicating whether the flight is impacted by at least one MIT restriction associated with this reroute.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="orig" type="nce:airportIdType" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Origin Airport</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="dCenter" type="nce:airportIdType" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Departure Center</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="dest" type="nce:airportIdType" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Destination (Arrival) Airport</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="aCenter" type="nce:airportIdType" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Destination (Arrival) Center</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="dFix" type="nce:fixNameType" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Departure Fix</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="aFix" type="nce:fixNameType" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Arrival Fix</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="igtd" type="xsd:dateTime" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Initial Gate Time of Departure</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="pgtd" type="xsd:dateTime" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Proposed Gate Time of Departure</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="etd" type="xsd:dateTime" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Estimated Time of Departure</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="eta" type="xsd:dateTime" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Estimated Time of Arrival</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ctd" type="xsd:dateTime" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Controlled Time of Departure</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="cta" type="xsd:dateTime" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Controlled Time of Arrival</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="status" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Flight Status - ETD Prefix: S - Scheduled. Flight data is based on OAG data. N - P-time from the airline-provided flight plan message. P - Proposed.  Flight data is based on flight plan. T - Taxi.  TFMS has received a message indicating the flight has pushed off the gate. A - Flight is active and the ETD is an actual departure time. E - Flight is active and the ETD is an estimate of the actual departure time. No departure message was received but the flight is known to be airborne because NAS messages indicating this have been received. R -- Flight's historical route has been replaced by its assigned reroute for modeling purposes.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:string">
                        <xsd:pattern value="(S|N|P|T|A|E|R)"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="ctlElem" type="xsd:string" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Control Element</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="eType" type="xsd:string" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Equipment Type</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="acCat" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Aircraft Category. One of: J=Jet, P=Prop, T=Turbo</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="J"/>
                        <xsd:enumeration value="P"/>
                        <xsd:enumeration value="T"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="weightClass" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Weight Class. One of: H=Heavy, L=Large, S=Small</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="H"/>
                        <xsd:enumeration value="L"/>
                        <xsd:enumeration value="S"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="userCat" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>User Category. One of: T=Air Taxi, F=Cargo, C=Commercial, G=General Aviation, M=Military</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="T"/>
                        <xsd:enumeration value="F"/>
                        <xsd:enumeration value="C"/>
                        <xsd:enumeration value="G"/>
                        <xsd:enumeration value="M"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="speed" type="xsd:int" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Speed in knots</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="alt" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Requested flight level 000 - 600</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:int">
                        <xsd:minInclusive value="0"/>
                        <xsd:maxInclusive value="600"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="currentRoute" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Current route string. 256 char maximum.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="256"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="nextWaypoint" type="xsd:int" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Index of the next waypoint that the flight will reach. First waypoint has an index value of 0 and this will be the value if the flight is not active.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ete" type="xsd:int" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Estimated Time Enroute along the current route - minutes</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="distance" type="xsd:int" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Distance along current route in nautical miles</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="currentLat" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Current latitude - hundredths of deg</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:int">
                        <xsd:minInclusive value="-9000"/>
                        <xsd:maxInclusive value="9000"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="currentLon" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Current longitude - hundredths of deg</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:int">
                        <xsd:maxInclusive value="18000"/>
                        <xsd:minInclusive value="-18000"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="ctrWaypoints" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Container for the data for each waypoint</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="ctrWaypoint" maxOccurs="unbounded">
                          <xsd:annotation>
                            <xsd:documentation>Container for the data for each waypoint</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element name="waypointIndex" type="xsd:int">
                                <xsd:annotation>
                                  <xsd:documentation>Index of the waypoint. The first waypoint has an index value of 0.</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="latLon" type="rriaLatLongType">
                                <xsd:annotation>
                                  <xsd:documentation>Latitude, Longitude of the waypoint in hundredths of degrees</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="ctrCenterList" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Container for the list of center entries</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="ctrCenter" maxOccurs="unbounded">
                          <xsd:annotation>
                            <xsd:documentation>Container for each center entry</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element name="centerName" type="nce:airportIdType">
                                <xsd:annotation>
                                  <xsd:documentation>Name of the center</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="centerEntry" type="xsd:dateTime">
                                <xsd:annotation>
                                  <xsd:documentation>Time of center entry</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="ctrAssignedRte" maxOccurs="unbounded">
                    <xsd:annotation>
                      <xsd:documentation>Container for assigned route definition</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="includeRte" type="xsd:boolean">
                          <xsd:annotation>
                            <xsd:documentation>Indicates whether this assigned route for this flight is marked as included in the flight list.</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="includeSegment" type="xsd:boolean">
                          <xsd:annotation>
                            <xsd:documentation>If at least one route segment associated with this assigned route is marked as excluded('false') in the reroute definition; otherwise value is 'true'.</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="assignedRte">
                          <xsd:annotation>
                            <xsd:documentation>Assigned route for the flight. Up to 1024 characters. Note that the route elements are separated by spaces. If the flight has no assigned route, the value will be "NO ASSIGNED ROUTE" and most other tags associated with this container will not be present in the file for this flight.</xsd:documentation>
                          </xsd:annotation>
                          <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                              <xsd:maxLength value="1024"/>
                            </xsd:restriction>
                          </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="assignedRteType">
                          <xsd:annotation>
                            <xsd:documentation>Route type of assigned route.  One of NONE, CRD RTE, RERTE, UNKN RTE, UPT RTE.</xsd:documentation>
                          </xsd:annotation>
                          <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                              <xsd:enumeration value="NONE"/>
                              <xsd:enumeration value="CRD RTE"/>
                              <xsd:enumeration value="RERTE"/>
                              <xsd:enumeration value="UNKN RTE"/>
                              <xsd:enumeration value="UPT RTE"/>
                              <xsd:enumeration value="ASSIGNED:"/>
                            </xsd:restriction>
                          </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="segmentId" minOccurs="0" type="rriaCombinedSegmentIdType">
                          <xsd:annotation>
                            <xsd:documentation>For a full route, the route grid row in which this segment appears. For a split route, a space-separated pair of route grid rows, first for the origin row, and second for the destination row.  Rows in the upper (origin) grid begin with "O", and rows in the lower (destination) grid begin with "D".  Within each grid, rows are sequentially numbered beginning with 0.  That is, Origin segments rows are O0, O1 etc., while Destination segment rows are D0, D1 etc. Up to 6 characters.</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="modeledRte" type="xsd:string" minOccurs="0">
                          <xsd:annotation>
                            <xsd:documentation>End-to-end route used to model the flight using this assigned route.</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="usedForModeling" type="xsd:boolean" minOccurs="0">
                          <xsd:annotation>
                            <xsd:documentation>Indicates whether this assigned route was used for this flight for modeling the impact of the reroute on the NAS.  Can be TRUE for at most one CTR_ASSIGNED_RTE per flight.</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="ctrModeledWaypoints" minOccurs="0">
                          <xsd:annotation>
                            <xsd:documentation>Container for the list of waypoints along the end-to-end route used to model the flight using this assigned route.</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element name="ctrWaypoint" minOccurs="2" maxOccurs="unbounded">
                                <xsd:annotation>
                                  <xsd:documentation>Container for the waypoint data for the modeled route.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                  <xsd:sequence>
                                    <xsd:element name="waypointIndex" type="xsd:int">
                                      <xsd:annotation>
                                        <xsd:documentation>Index of the waypoint. The first waypoint has an index value of 0.</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="latLon" type="rriaLatLongType">
                                      <xsd:annotation>
                                        <xsd:documentation>Latitude, Longitude of the waypoint in hundredths of degrees</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="crossingTime" type="xsd:dateTime">
                                      <xsd:annotation>
                                        <xsd:documentation>Time of crossing the waypoint without any MIT restrictions</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="crossingTimeWithMit" type="xsd:dateTime" minOccurs="0">
                                      <xsd:annotation>
                                        <xsd:documentation>Time of crossing the waypoint with MIT restrictions.  Present only if IMPACTED_BY_MIT is Y for this flight.</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                  </xsd:sequence>
                                </xsd:complexType>
                              </xsd:element>
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="nextModeledWaypoint" type="xsd:int" minOccurs="0">
                          <xsd:annotation>
                            <xsd:documentation>Index of the next waypoint in the modeled flight trajectory that the flight will reach. First waypoint has an index value of 0 and this will be the value if the flight is not active. Note: This is only present in the assigned route used for modeling</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="modeledEte" type="xsd:int" minOccurs="0">
                          <xsd:annotation>
                            <xsd:documentation>Estimated Time Enroute using the route modeled for this assigned route.</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="MitModeledEte" type="xsd:int" minOccurs="0">
                          <xsd:annotation>
                            <xsd:documentation>Estimated Time Enroute with MIT restrictions using the end-to-end route modeled for this assigned route. Note:  This is only present in the assigned route used for modeling, and only if IMPACTED_BY_MIT is Y for this flight.</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="modeledDistance" type="xsd:int" minOccurs="0">
                          <xsd:annotation>
                            <xsd:documentation>Estimated Distance Enroute using the end-to-end route modeled for this assigned route.</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="ctrMitRestrictionsList" minOccurs="0">
                          <xsd:annotation>
                            <xsd:documentation>Container for list of MIT restrictions associated with this flight. Only present if flight is impacted by at least one MIT associated with this reroute.  If the flight is included in the reroute, CTR_MIT_RESTRICTION_LIST is present only in the CTR_ASSIGNED_RTE for the assigned route that was used for modeling.  If there is no such assigned route, CTR_MIT_RESTRICTION_LIST is absent. If a flight is not in the reroute but is still subject to the MIT (which can happen if the MIT is based on an FEA),  CTR_MIT_RESTRICTION_LIST is contained in CTR_FLIGHT, since in this case there is no assigned route for the flight, and the MIT is applied to the flight's current route.</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element name="ctrMitRestriction" type="ctrMitRestrictionType" maxOccurs="unbounded">
                                <xsd:annotation>
                                  <xsd:documentation>Container for single MIT restriction. Present only for each MIT associated with this reroute that applies to this flight.</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="ctrModeledCenterList" minOccurs="0">
                          <xsd:annotation>
                            <xsd:documentation>Container for the list of centers traversed by the route modeled for this assigned route.</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element name="ctrCenter" maxOccurs="unbounded">
                                <xsd:annotation>
                                  <xsd:documentation>Container for the data for each center</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                  <xsd:sequence>
                                    <xsd:element name="centerName" type="nce:airportIdType">
                                      <xsd:annotation>
                                        <xsd:documentation>Name of the center</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="centerEntry" type="xsd:dateTime">
                                      <xsd:annotation>
                                        <xsd:documentation>Time of center entry without MIT restrictions</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="centerEntryWithMit" type="xsd:dateTime" minOccurs="0">
                                      <xsd:annotation>
                                        <xsd:documentation>Time of center entry with MIT restrictions</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                  </xsd:sequence>
                                </xsd:complexType>
                              </xsd:element>
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="ctrMitRestrictionsList" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Container for list of MIT restrictions associated with this flight. Only present if flight is impacted by at least one MIT associated with this reroute. If the flight is included in the reroute, CTR_MIT_RESTRICTION_LIST is present only in the CTR_ASSIGNED_RTE for the assigned route that was used for modeling.  If there is no such assigned route, CTR_MIT_RESTRICTION_LIST is absent. If a flight is not in the reroute but is still subject to the MIT (which can happen if the MIT is based on an FEA), CTR_MIT_RESTRICTION_LIST is contained in CTR_FLIGHT, since in this case there is no assigned route for the flight, and the MIT is applied to the flight's current route.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="ctrMitRestriction" type="ctrMitRestrictionType" maxOccurs="unbounded">
                          <xsd:annotation>
                            <xsd:documentation>Container for single MIT restriction. Present only for each MIT associated with this reroute that applies to this flight.</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ctrMitTimesList" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Container for MIT start and end times, as computed by the RRIA back-end.  Present only when MIT_TIME_TYPE is AUTO for at least one MIT associated with this reroute.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="ctrMitTime" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Container for computed start and end times for an MIT and each filter associated with the MIT that has specified spacing (not "AS_ONE").  Present for each MIT for which MIT_TIME_TYPE is AUTO.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="mitId">
                    <xsd:annotation>
                      <xsd:documentation>MIT ID</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:string">
                        <xsd:maxLength value="30"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="mitFeaFilterId" type="rriaIdType" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>FEA filter ID present if separate spacing has been specified for flights that match an FEA secondary filter.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="mitDestinationFilterId" type="nce:airportIdType" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Airport name, present if separate spacing has been specified for flights that are going to a specified destination airport.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="mitStartTime" type="xsd:dateTime">
                    <xsd:annotation>
                      <xsd:documentation>Start time for the MIT restriction, as computed by the RRIA back-end.6</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="mitEndTime" type="xsd:dateTime">
                    <xsd:annotation>
                      <xsd:documentation>End time for the MIT restriction, as computed by the RRIA back-end.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ctrPointDataCommon">
    <xsd:annotation>
      <xsd:documentation>Point data common.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="todayMapOn" type="rriaCountsPositiveType">
        <xsd:annotation>
          <xsd:documentation>Current Monitor Alert Parameter value for each interval.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="alertStatusBefore" type="rriaAlertStatusType">
        <xsd:annotation>
          <xsd:documentation>RYG alert status of the fix for each interval without the reroute</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="alertStatusAfter" type="rriaAlertStatusType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>RYG alert status of the fix for each interval with the reroute. Only present for fixes that are impacted by the reroute.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="alertStatusAfterMit" type="rriaAlertStatusType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>RYG alert status of the fix for each interval with the MIT restrictions. Only present for fixes that are impacted by an MIT restriction.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="totalAllAfter" type="rriaCountsPositiveType">
        <xsd:annotation>
          <xsd:documentation>Total count of all (currently active and proposed) traffic that traverses the fix in each interval with the reroute</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="totalActiveAfter" type="rriaCountsPositiveType">
        <xsd:annotation>
          <xsd:documentation>Total count of all currently active traffic that traverses the fix for each interval with the reroute</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="totalAllDelta" type="rriaCountsDeltaType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Difference in the total count of all (currently active and proposed) traffic that traverses the fix in each interval with and without the reroute. Positive delta indicates an increase in the count with the reroute. Only present for fixes that are impacted by the reroute.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="totalActiveDelta" type="rriaCountsDeltaType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Difference in the Total count of all currently active traffic that traverses the fix for each interval with and without the reroute. Positive delta indicates an increase in the count with the reroute. Only present for fixes that are impacted by the reroute.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="totalAllAfterMit" type="rriaCountsPositiveType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Total count of all (currently active and proposed) traffic that traverses the fix in each interval with the reroute. Only present for fixes that are impacted by the MIT restrictions.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="totalActiveAfterMit" type="rriaCountsPositiveType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Total count of all currently active traffic that traverses the fix for each interval with the reroute. Only present for fixes that are impacted by the MIT restrictions.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="totalAllMitDelta" type="rriaCountsDeltaType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Difference in the total count of all (currently active and proposed) traffic that traverses the fix in each interval with and without the reroute. Positive delta indicates an increase in the count with the reroute. Only present for fixes that are impacted by the MIT restrictions.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="totalActiveMitDelta" type="rriaCountsDeltaType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Difference in the Total count of all currently active traffic that traverses the fix for each interval with and without the reroute. Positive delta indicates an increase in the count with the reroute. Only present for fixes that are impacted by the MIT restrictions.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ctrEntryExitTimeCommon">
    <xsd:annotation>
      <xsd:documentation>FCA entry and exit times.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="entry" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>FCA entry time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="exit" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>FCA exit time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="filterFlags" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Indicates whether the flight satisfies the secondary filter conditions. One of 0 or 1. A value of 1 indicates the flight satisfies the filter conditions. Values are in filter number order starting at zero. Will not be present if there are no secondary filters for the FCA.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:pattern value="(0,|1,)*[01]"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="rriaTrafficCountsDataType">
    <xsd:annotation>
      <xsd:documentation>Reroute model traffic counts data type.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ctrHead">
        <xsd:annotation>
          <xsd:documentation>Container for the file header data.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="rerouteId" type="rerouteIdType">
              <xsd:annotation>
                <xsd:documentation>System-generated reroute identifier. Up to 64 characters.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="rerouteName" type="rerouteNameType">
              <xsd:annotation>
                <xsd:documentation>Reroute name. Up to 64 characters.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="modelTime" type="xsd:dateTime">
              <xsd:annotation>
                <xsd:documentation>Time when the modeling was requested.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="firstIntervalTime" type="xsd:dateTime">
              <xsd:annotation>
                <xsd:documentation>First minute of the first interval that has data in all enumerated fields.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="lastIntervalTime" type="xsd:dateTime">
              <xsd:annotation>
                <xsd:documentation>First minute of the last interval that has data in all enumerated fields.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="minutesInInterval" type="xsd:int" default="15">
              <xsd:annotation>
                <xsd:documentation>Normally 15 minutes.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ctrSectorList">
        <xsd:annotation>
          <xsd:documentation>Container for the alert status and traffic count data for all sectors.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="ctrCenter" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Container for the data for a single center</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="centerName" type="nce:airportIdType">
                    <xsd:annotation>
                      <xsd:documentation>Name of the center</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="impactedByReroute" type="xsd:boolean">
                    <xsd:annotation>
                      <xsd:documentation>Flag indicating whether any sector in the center is impacted by the reroute. One of Y or N.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="impactedByMit" type="xsd:boolean">
                    <xsd:annotation>
                      <xsd:documentation>Flag indicating whether any sector in the center is impacted by at least one of the MIT restrictions associated with this reroute.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ctrSector" maxOccurs="unbounded">
                    <xsd:annotation>
                      <xsd:documentation>Container for the alert status and traffic count data for a single sector.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="sectorType">
                          <xsd:annotation>
                            <xsd:documentation>Type of sector. One of lowsec, highsec, superhisec.</xsd:documentation>
                          </xsd:annotation>
                          <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                              <xsd:enumeration value="lowsec"/>
                              <xsd:enumeration value="highsec"/>
                              <xsd:enumeration value="superhisec"/>
                            </xsd:restriction>
                          </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="sectorName">
                          <xsd:annotation>
                            <xsd:documentation>Name of the sector</xsd:documentation>
                          </xsd:annotation>
                          <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                              <xsd:maxLength value="10"/>
                              <xsd:pattern value="[A-Za-z0-9]*"/>
                            </xsd:restriction>
                          </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="sectorState">
                          <xsd:annotation>
                            <xsd:documentation>One of BASELINE, DYNAMIC, CLOSED</xsd:documentation>
                          </xsd:annotation>
                          <xsd:simpleType>
                            <xsd:restriction base="xsd:string">
                              <xsd:enumeration value="BASELINE"/>
                              <xsd:enumeration value="DYNAMIC"/>
                              <xsd:enumeration value="CLOSED"/>
                            </xsd:restriction>
                          </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="sectorsAdded" type="xsd:string" minOccurs="0">
                          <xsd:annotation>
                            <xsd:documentation>List of sectors that are included in whole or in part in this sector if any portion of another sector has been added to this sector</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="impactedByReroute" type="xsd:boolean">
                          <xsd:annotation>
                            <xsd:documentation>Indicates that the sector has been impacted by the reroute. (At least one flight is added to the sector, removed from the sector, or time shifted in the sector due to the reroute.)</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="impactedByMit" type="xsd:boolean">
                          <xsd:annotation>
                            <xsd:documentation>Flag indicating whether the sector has been impacted by the MIT restrictions. (At least one flight is time shifted in the sector due to the MIT restrictions associated with this reroute.)</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                        <xsd:sequence>
                          <xsd:element name="ctrBaselineData" type="ctrSectorDataCommon">
                            <xsd:annotation>
                              <xsd:documentation>Container for the traffic counts of the sector in its baseline state</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                          <xsd:element name="ctrDynamicData" type="ctrSectorDataCommon" minOccurs="0">
                            <xsd:annotation>
                              <xsd:documentation>Container for the traffic counts of the sector in its dynamic state</xsd:documentation>
                            </xsd:annotation>
                          </xsd:element>
                        </xsd:sequence>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ctrFixList" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Container for the alert status and traffic count data for all alerted fixes.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="ctrFix" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Container for the alert status and traffic count data for a single fix.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="fixName" type="nce:fixNameType">
                    <xsd:annotation>
                      <xsd:documentation>Name of the fix</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="impactedByReroute" type="xsd:boolean">
                    <xsd:annotation>
                      <xsd:documentation>Indicates that the fix has been impacted by the reroute. (At least one flight crosses the fix, no longer crosses the fix or crosses the fix at a different time, due to the reroute.)</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="impactedByMit" type="xsd:boolean">
                    <xsd:annotation>
                      <xsd:documentation>Indicates that the fix has been impacted by the MIT restrictions. (At least one flight crosses the fix at a different time due to the reroute.)</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ctrLowData" type="ctrPointDataCommon">
                    <xsd:annotation>
                      <xsd:documentation>Container for the alert status and traffic count data for the low altitude strata for the fix</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ctrHighData" type="ctrPointDataCommon">
                    <xsd:annotation>
                      <xsd:documentation>Container for the alert status and traffic count data for the high altitude strata for the fix</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ctrSuperHighData" type="ctrPointDataCommon">
                    <xsd:annotation>
                      <xsd:documentation>Container for the alert status and traffic count data for the superhigh altitude strata for the fix</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ctrAirportList">
        <xsd:annotation>
          <xsd:documentation>Container for the alert status and traffic count data for all pacing and alerted airports.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="ctrAirport" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Container for the alert status and traffic count data for a single airport.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="airportName" type="nce:airportIdType">
                    <xsd:annotation>
                      <xsd:documentation>Name of the airport.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="impactedByReroute" type="xsd:boolean">
                    <xsd:annotation>
                      <xsd:documentation>Indicates that the airport has been impacted by the reroute (the arrival time at this airport for at least one flight is different due to the reroute).</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="impactedByMit" type="xsd:boolean">
                    <xsd:annotation>
                      <xsd:documentation>Indicates that the airport has been impacted by the MIT restrictions. (The departure time or arrival time at this airport is different for at least one flight due to the MIT associated with this reroute.)</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:sequence>
                    <xsd:element name="ctrDepartureData" type="ctrPointDataCommon">
                      <xsd:annotation>
                        <xsd:documentation>Container for the alert status and traffic count data for the airport departures.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                    <xsd:element name="ctrArrivalData" type="ctrPointDataCommon">
                      <xsd:annotation>
                        <xsd:documentation>Container for the alert status and traffic count data for the airport arrivals.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:element>
                  </xsd:sequence>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="rriaFCAFlightsDataType">
    <xsd:annotation>
      <xsd:documentation>Reroute model FCA flight data list type.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ctrHead">
        <xsd:annotation>
          <xsd:documentation>Container for the header data.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="fcaId" type="rriaIdType">
              <xsd:annotation>
                <xsd:documentation>System generated identifier of the FEA/FCA</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="fcaName">
              <xsd:annotation>
                <xsd:documentation>Name of the FCA</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="30"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="fcaStartTime" type="xsd:dateTime">
              <xsd:annotation>
                <xsd:documentation>Start time of the FEA/FCA</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="fcaEndTime" type="xsd:dateTime">
              <xsd:annotation>
                <xsd:documentation>End time of the FEA/FCA</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="rerouteId" type="rriaIdType">
              <xsd:annotation>
                <xsd:documentation>System-generated reroute identifier. Up to 64 characters.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="rerouteName" type="rerouteNameType">
              <xsd:annotation>
                <xsd:documentation>Reroute name. Up to 64 characters.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="modelTime" type="xsd:dateTime">
              <xsd:annotation>
                <xsd:documentation>Time when the modeling was requested.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ctrFcaFilterList" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Container for the list of secondary filters for the FCA</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="ctrFilter" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Secondary filter list.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="filterFlagNumber" type="xsd:int">
                    <xsd:annotation>
                      <xsd:documentation>Secondary filter number. Numbers start at zero.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="filterId" type="rriaIdType">
                    <xsd:annotation>
                      <xsd:documentation>Secondary filter ID.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ctrFlightList">
        <xsd:annotation>
          <xsd:documentation>Container for the FCA reroute modeling flight list</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="ctrFlight" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Flight data.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="acid" type="nce:aircraftIdType">
                    <xsd:annotation>
                      <xsd:documentation>Aircraft ID</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="active" type="xsd:boolean">
                    <xsd:annotation>
                      <xsd:documentation>Indicates whether the flight is currently active.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ctrBefore" type="ctrEntryExitTimeCommon" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Container for the FCA entry and exit times of the flight without the reroute. Will not be present if the flight does not intersect the FCA without the reroute.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ctrAfter" type="ctrEntryExitTimeCommon" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Container for the FCA entry and exit times of the flight with the reroute without the MIT restrictions. Will not be present if the flight does not intersect the FCA with the reroute.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ctrAfterMit" type="ctrEntryExitTimeCommon" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Container for the FCA entry and exit times of the flight with the reroute and MIT restrictions. Will not be present if the flight is not impacted by the MIT restrictions.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="rriaTMIListType">
    <xsd:annotation>
      <xsd:documentation>Reroute model TMI list type.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="gdpList" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List of airports with a GDP that has been issued, modified, or cancelled since the specified time.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="gdp" type="tmiDataType" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Ground Delay Programs.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="gsList" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List of airports with an active Ground Stop</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="gs" type="tmiDataType" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Ground Stops.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="afpList" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List of FCAs with an active AFP</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="afp" type="tmiDataType" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Airspace Flow Programs.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="rriaModelDataType">
