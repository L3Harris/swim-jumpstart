      <xsd:enumeration value="EDCTSCSTGL"/>
      <xsd:enumeration value="EDCTSHOW"/>
      <xsd:enumeration value="EDCTSUBSHOW"/>
      <xsd:enumeration value="EDCTSUBTGL"/>
      <xsd:enumeration value="EDCTSLIST"/>
      <xsd:enumeration value="EDCTUPDT"/>
      <xsd:enumeration value="EDCTUNSLT"/>
      <xsd:enumeration value="FADT"/>
      <xsd:enumeration value="FDBLOCK"/>
      <xsd:enumeration value="FLTRECON"/>
      <xsd:enumeration value="FSCANC"/>
      <xsd:enumeration value="FSACT"/>
      <xsd:enumeration value="FSINHB"/>
      <xsd:enumeration value="FSREMV"/>
      <xsd:enumeration value="FSRESTR"/>
      <xsd:enumeration value="FSUPDT"/>
