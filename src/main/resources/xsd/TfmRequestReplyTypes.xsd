    <xsd:sequence>
      <xsd:element name="modelSharing" type="xsd:boolean">
        <xsd:annotation>
          <xsd:documentation>"true" if the RRIA_BE is to send the Model Availability message to RDPS or RRSVR.  "false" if the RRIA_BE should not send the Model Availability message.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="rerouteDefinition" type="rriaRerouteType">
        <xsd:annotation>
