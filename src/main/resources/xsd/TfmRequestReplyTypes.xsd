      <xsd:element name="rerouteId" type="rerouteIdType">
        <xsd:annotation>
          <xsd:documentation>Unique identifier of the reroute. rr.site.workstation.creation date/time</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="rriaTrafficCounts" type="rriaTrafficCountsDataType">
        <xsd:annotation>
          <xsd:documentation>Model data.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="rriaPreviewReplyMsgType">
    <xsd:annotation>
      <xsd:documentation>Reroute model: Preview request type.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="rerouteId" type="rerouteIdType">
        <xsd:annotation>
          <xsd:documentation>Unique identifier of the reroute</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="flightList" type="rriaFlightsDataType">
