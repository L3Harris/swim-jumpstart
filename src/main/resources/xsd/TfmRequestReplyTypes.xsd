<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns="urn:us:gov:dot:faa:atm:tfm:tfmrequestreplytypes" 
            xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
            xmlns:nce="urn:us:gov:dot:faa:atm:tfm:tfmdatacoreelements" 
            xmlns:fce="urn:us:gov:dot:faa:atm:tfm:ficommondatatypes" 
            xmlns:fcm="urn:us:gov:dot:faa:atm:tfm:ficommonmessages" 
            xmlns:fm2="urn:us:gov:dot:faa:atm:tfm:ficommonmessages2" 
            xmlns:fdm="urn:us:gov:dot:faa:atm:tfm:flightdatacommonmessages" 
            targetNamespace="urn:us:gov:dot:faa:atm:tfm:tfmrequestreplytypes" 
            version="2.0.5" 
            elementFormDefault="qualified" 
            attributeFormDefault="unqualified">
  <xsd:import namespace="urn:us:gov:dot:faa:atm:tfm:ficommondatatypes" schemaLocation="FICommonDataTypes.xsd"/>
  <xsd:import namespace="urn:us:gov:dot:faa:atm:tfm:ficommonmessages" schemaLocation="FICommonMessages.xsd"/>
  <xsd:import namespace="urn:us:gov:dot:faa:atm:tfm:ficommonmessages2" schemaLocation="TfmFICmnMessages2.xsd"/>
  <xsd:import namespace="urn:us:gov:dot:faa:atm:tfm:tfmdatacoreelements" schemaLocation="TFMDataCoreElements.xsd"/>
  <xsd:import namespace="urn:us:gov:dot:faa:atm:tfm:flightdatacommonmessages" schemaLocation="FlightDataCommonMessages.xsd"/>
  <xsd:simpleType name="aircraftRegMarkType">
    <xsd:annotation>
      <xsd:documentation>Aircraft Registration Mark La[a][a][a][a][a].</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[A-Z][a-z|0-9]{1,6}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="activateCommandType">
    <xsd:annotation>
      <xsd:documentation>Defines the Activate command type.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:choice>
        <xsd:element name="airlineId" type="nce:airlineIdType">
          <xsd:annotation>
            <xsd:documentation>Three-letter airline code. Either Airline or Flight is required, but not both.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="flightId" type="nce:aircraftIdType">
          <xsd:annotation>
            <xsd:documentation>Specifies either the flight number or the airline  to inhibit or activate.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:choice>
      <xsd:sequence minOccurs="0">
        <xsd:element name="startDate" type="xsd:date">
          <xsd:annotation>
            <xsd:documentation>The date on which the command is to take effect.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="stopDate" type="xsd:date" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>The date on which the effect of the command is to be removed.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="arptRequestType">
    <xsd:annotation>
      <xsd:documentation>Defines a request for flight data associated with an airport.</xsd:documentation>
    </xsd:annotation>
    <xsd:choice>
      <xsd:element name="register">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="duration">
              <xsd:annotation>
                <xsd:documentation>Duration of registration. Registration is terminated after the specified hours.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:integer">
                  <xsd:minInclusive value="1"/>
                  <xsd:maxInclusive value="24"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="airports">
              <xsd:annotation>
                <xsd:documentation>Airports to be monitored.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="monType">
                    <xsd:annotation>
                      <xsd:documentation>Airports to be monitored.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="AIRPORT_ARRIVALS"/>
                        <xsd:enumeration value="AIRPORT_DEPARTURES"/>
                        <xsd:enumeration value="AIRPORT_ALL"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="airport" type="nce:airportIdType" maxOccurs="50">
                    <xsd:annotation>
                      <xsd:documentation>Identifies the airport.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="renew">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="duration">
              <xsd:annotation>
                <xsd:documentation>Duration of renewal of the registration. Registration is terminated after the specified hours.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:integer">
                  <xsd:minInclusive value="1"/>
                  <xsd:maxInclusive value="24"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="airports">
              <xsd:annotation>
                <xsd:documentation>Airports to be monitored.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="monType">
                    <xsd:annotation>
                      <xsd:documentation>Airports to be monitored.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="AIRPORT_ARRIVALS"/>
                        <xsd:enumeration value="AIRPORT_DEPARTURES"/>
                        <xsd:enumeration value="AIRPORT_ALL"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="airport" type="nce:airportIdType" maxOccurs="50">
                    <xsd:annotation>
                      <xsd:documentation>Identifies the airport.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="deregister">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="airports">
              <xsd:annotation>
                <xsd:documentation>Airports to be monitored.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="monType">
                    <xsd:annotation>
                      <xsd:documentation>Airports to be monitored.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="AIRPORT_ARRIVALS"/>
                        <xsd:enumeration value="AIRPORT_DEPARTURES"/>
                        <xsd:enumeration value="AIRPORT_ALL"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="airport" type="nce:airportIdType" maxOccurs="50">
                    <xsd:annotation>
                      <xsd:documentation>Identifies the airport.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>
  <xsd:simpleType name="runwayNameType">
    <xsd:annotation>
      <xsd:documentation>A number between 01 and 36 AND optionally  Left (L), Center (C), or Right (R).</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="(0[1-9]|[12][0-9]|3[0-6])[LRC]{0,1}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="airlineSurfaceDataType">
    <xsd:sequence>
      <xsd:element name="IOBT" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Initial Off-Block Time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EOBT" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Field T15 Earliest Off-Block Time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="depStandAssignment" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Field T16 Departure Stand Assignment Ldd[d]- Gate.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="arrStandAssignment" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Field T17 ArrivalStand Assignment Ldd[d]- Gate.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="arrStandAvailable" type="xsd:boolean" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Field T18 Arrival Stand Availability.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="tmatRelinquish" type="xsd:boolean" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Field T19 TMAT Relinquish Indication.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="tmatMarkedForSub" type="xsd:boolean" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Field T20 TMAT Marked For Substitution Indication.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="aircraftRegMark" type="aircraftRegMarkType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Field T21 Aircraft Registration Mark.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:sequence minOccurs="0">
        <xsd:element name="accDepRwy" type="runwayNameType" maxOccurs="7">
          <xsd:annotation>
            <xsd:documentation>Field T22 List of Acceptable Departure Runways.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:sequence minOccurs="0">
        <xsd:element name="unaccDepRwy" type="runwayNameType" maxOccurs="7">
          <xsd:annotation>
            <xsd:documentation>Field T23 List of Unacceptable Departure Runways.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:element name="depReadiness" type="xsd:boolean" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Field T24 Departure Readiness Status.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="depIntentToHoldNonMove" type="xsd:boolean" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Field T25 Intent To Hold In The Airport Non-Movement Area During Departure.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="arrIntentToHoldNonMove" type="xsd:boolean" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Field T26 Intent To Hold In The Airport Non-Movement Area During Arrival.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="depIntentToHoldInMove" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Field T27 Intent To Hold In the Airport Movement Area During Departure.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:simpleContent>
            <xsd:extension base="xsd:boolean">
              <xsd:attribute name="holdIntent">
                <xsd:simpleType>
                  <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="HOLD"/>
                    <xsd:enumeration value="NO_HOLD"/>
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:attribute>
              <xsd:attribute name="estimatedEntryTime" type="xsd:dateTime"/>
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="arrIntentToHoldInMove" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Field T28 Intent To Hold In the Airport Movement Area During Arrival.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:simpleContent>
            <xsd:extension base="xsd:boolean">
              <xsd:attribute name="holdIntent">
                <xsd:simpleType>
                  <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="HOLD"/>
                    <xsd:enumeration value="NO_HOLD"/>
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:attribute>
              <xsd:attribute name="estimatedExitTime" type="xsd:dateTime"/>
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="intentToBeDeiced" type="xsd:boolean" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Field T29 Intent For A Flight To Be De-Iced.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="intendedDeiceLocation" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Field T30 Intended De-Icing Location Ldd[d]- Gate.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="intendedDepSpot" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Field T31 Intended Departure Spot Ldd[d] "Gate".</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="intendedArrSpot" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Field T32 Intended Arrival Spot Ldd[d]- Gate.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="intentReturnToGate" type="xsd:boolean" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Field T33 Gate Return Intent.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AOBT" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Field T34 Actual Off-Block Time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="AIBT" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Field T35 Actual In-Block Time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ATOT" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Field T36 Actual Take-Off Time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ALTD" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Field T37 Actual Landing Time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="airportFixReqType">
    <xsd:annotation>
      <xsd:documentation>Defines a request for airport arrival/departure fixes.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="airport" type="nce:airportIdType" maxOccurs="50">
        <xsd:annotation>
          <xsd:documentation>Identifies the airport.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="airportFixDataType">
    <xsd:annotation>
      <xsd:documentation>Defines the requested for airport.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="airport" maxOccurs="50">
        <xsd:annotation>
          <xsd:documentation>Identifies the airport.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="arrFix" type="nce:fixNameType" minOccurs="0" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Identifies zero or more arrival fixes.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="depFix" type="nce:fixNameType" minOccurs="0" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>Identifies zero or more departure fixes.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
          <xsd:attribute name="name" type="nce:airportIdType"/>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:attributeGroup name="controlTimePairType">
    <xsd:annotation>
      <xsd:documentation>Defines the control time pair type.</xsd:documentation>
    </xsd:annotation>
    <xsd:attribute name="CTD" type="xsd:dateTime" use="required">
      <xsd:annotation>
        <xsd:documentation>Controlled time of departure.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="CTA" type="xsd:dateTime" use="required">
      <xsd:annotation>
        <xsd:documentation>Controlled time of arrival.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:attributeGroup>
  <xsd:complexType name="ctopManualOverrideType">
    <xsd:annotation>
      <xsd:documentation>Defines the CTOP manual override type.</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element name="CTOP_IDFR" type="fce:idfrType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Unique CTOP identifier or reference number.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="FLIGHT_IDFR" type="fce:idfrType">
        <xsd:annotation>
          <xsd:documentation>Unique flight identifier.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ALTITUDE" type="fm2:trajAltTypeHundred">
        <xsd:annotation>
          <xsd:documentation>Flight altitude.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ROUTE">
        <xsd:annotation>
          <xsd:documentation>The route of flight as it would be submitted on a flight plan in Field 10 format</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="1000"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="SPEED">
        <xsd:annotation>
          <xsd:documentation>Cruising speed in knots.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9]{3,4}"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="EDCT" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>If an EDCT is not supplied for the override, TFMS will generate one.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TRAJ_INDEX" type="fcm:trajIndexType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Index used to differentiate the trajectory options for the purpose of generating replies.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OVERRIDE_TIME" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
      Indicates the time at which the manual override was issued.
     </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ERTD" minOccurs="0" type="xsd:dateTime"/>
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="ctopProcessingStatusUpdateType">
    <xsd:annotation>
      <xsd:documentation>Defines the CTOP processing status update type.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="delayTable" type="delayTableType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Delay table.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="updateText" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The update text message.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string"/>
        </xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="status" type="progressStatusType" use="required">
      <xsd:annotation>
        <xsd:documentation>Progress status..</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="ctopShortName" type="ctopTmiShortNameType">
      <xsd:annotation>
        <xsd:documentation>CTOP short name.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="ctopReqType">
    <xsd:annotation>
      <xsd:documentation>CTOP request types.</xsd:documentation>
    </xsd:annotation>
    <xsd:choice>
      <xsd:element name="deleteRequest">
        <xsd:annotation>
          <xsd:documentation>Used to cancel a CTOP program given a CTOP short name.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence maxOccurs="unbounded">
            <xsd:element name="CTOP">
              <xsd:annotation>
                <xsd:documentation>Identifies the CTOP TMI.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="CTOP_SHORT_NAME" type="fm2:ctopTmiShortNameType">
                    <xsd:annotation>
                      <xsd:documentation>The system generated short name used to identify the CTOP TMI.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="VERSION" type="xsd:int">
                    <xsd:annotation>
                      <xsd:documentation>CTOP TMI version.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="modelRequest">
        <xsd:annotation>
          <xsd:documentation>Used to request modeling results for the CTOP definition.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="CTOP_DATA" type="reqCtopTmiModelType">
              <xsd:annotation>
                <xsd:documentation>The CTOP definition as loaded from the definition file.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="overrideRequest" type="ctopManualOverrideType">
        <xsd:annotation>
          <xsd:documentation>The override request message sends a route in field 10 format to be applied to a specific flight unique IDFR.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="overridePreviewRequest" type="ctopManualOverrideType">
        <xsd:annotation>
          <xsd:documentation>The OVERRIDE_CK_ID verifies a route that the TSD user is considering for a manual override (override preview).</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="edctModelRequest">
        <xsd:annotation>
          <xsd:documentation>The TRS user submits an EDCT for verification.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="CTOP_IDFR" type="fce:idfrType">
              <xsd:annotation>
                <xsd:documentation>Unique CTOP TMI identifier.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="FLIGHT_IDFR" type="fce:idfrType">
              <xsd:annotation>
                <xsd:documentation>Unique flight identifier.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="EDCT" type="xsd:dateTime">
              <xsd:annotation>
                <xsd:documentation>Date and time representing the EDCT.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="MAX_DELAY" type="xsd:int">
              <xsd:annotation>
                <xsd:documentation>Minutes of delay the TM is willing to have applied to the EDCT.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="edctUpdateRequest">
        <xsd:annotation>
          <xsd:documentation>Used to override a flight's EDCT.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="CTOP_IDFR" type="fce:idfrType">
              <xsd:annotation>
                <xsd:documentation>Unique CTOP TMI identifier.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="FLIGHT_IDFR" type="fce:idfrType">
              <xsd:annotation>
                <xsd:documentation>Unique flight identifier.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="EDCT" type="xsd:dateTime">
              <xsd:annotation>
                <xsd:documentation>Date and time representing the EDCT.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="ERTD" minOccurs="0" type="xsd:dateTime"/>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="flightHistoryRequest">
        <xsd:annotation>
          <xsd:documentation>A request for the flight history as specified by the IDFR or the CDM information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="ctopReference" type="fce:idfrType">
              <xsd:annotation>
                <xsd:documentation>CTOP TFMS database reference number.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="ctopId" type="fcm:ctopTmiIdType">
              <xsd:annotation>
                <xsd:documentation>CTOP TMI identification.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:choice>
              <xsd:element name="FLIGHT_IDFR" type="fce:idfrType">
                <xsd:annotation>
                  <xsd:documentation>Unique flight identifier.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:sequence>
                <xsd:element name="ACID">
                  <xsd:annotation>
                    <xsd:documentation>The Aircraft ID.</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                      <xsd:maxLength value="7"/>
                    </xsd:restriction>
                  </xsd:simpleType>
                </xsd:element>
                <xsd:choice>
                  <xsd:element name="FLIGHT_DATE" type="xsd:date" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Original scheduled gate push back time for the flight.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="IGTD" type="xsd:dateTime" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Original scheduled gate push back time for the flight.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:choice>
              </xsd:sequence>
            </xsd:choice>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="cancelRequest">
        <xsd:annotation>
          <xsd:documentation>Cancels an active or proposed CTOP. The definition file is needed to provide the advisory text.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="ctopUniqueId" type="fm2:ctopUniqueIdType">
              <xsd:annotation>
                <xsd:documentation>The CTOP definition as loaded from the definition file.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="advisoryNumber" type="advisoryNumType"/>
            <xsd:element name="advisoryText">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:maxLength value="4000"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="activateRequest">
        <xsd:annotation>
          <xsd:documentation>Activates a proposed CTOP.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="CTOP_DATA" type="reqCtopTmiType">
              <xsd:annotation>
                <xsd:documentation>The CTOP definition as loaded from the definition file.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="proposeRequest">
        <xsd:annotation>
          <xsd:documentation>Proposes a private modeled CTOP.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="CTOP_DATA" type="reqCtopTmiType">
              <xsd:annotation>
                <xsd:documentation>The CTOP definition as loaded from the definition file.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ctopRankDemotionPreviewRequest" type="fm2:rankDemotionType">
        <xsd:annotation>
          <xsd:documentation>CTOP rank demotion preview request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ctopRankDemotionRequest" type="fm2:rankDemotionType">
        <xsd:annotation>
          <xsd:documentation>CTOP rank demotion request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>
  <xsd:simpleType name="ctopTmiShortNameType">
    <xsd:annotation>
      <xsd:documentation>System generated unique identifier for the CTOP</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="CTP\d{3}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="eteType">
    <xsd:annotation>
      <xsd:documentation>Specifies the estimated time (in minutes) enroute for this flight. 1 to 1440.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:integer">
      <xsd:maxInclusive value="1440"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="flightReconstitutionReqType">
    <xsd:annotation>
      <xsd:documentation>Type definition for request for flight data for the purpose of flight data recovery after a user outage or initial start of operation.</xsd:documentation>
    </xsd:annotation>
    <xsd:choice>
      <xsd:element name="general">
        <xsd:annotation>
          <xsd:documentation>Identifies that this is a general flight reconstitution request.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="active" type="xsd:boolean">
              <xsd:annotation>
                <xsd:documentation>If set true all flights with an "ACTIVE" status are included in the recovery.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="planned" type="xsd:boolean">
              <xsd:annotation>
                <xsd:documentation>If set true all flights with an "PLANNED" status are included in the recovery.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="canceled" type="xsd:boolean">
              <xsd:annotation>
                <xsd:documentation>If set true all flights with an "CANCELED" status are included in the recovery.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="completed" type="xsd:boolean">
              <xsd:annotation>
                <xsd:documentation>If set true all flights with an "COMPLETED" status are included in the recovery.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="timeSelect" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Optional time preference by providing either a from time or a specific time range; if the time option is not present all flights in the NCSM are provided.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:choice>
                  <xsd:element name="fromTime" type="xsd:dateTime">
                    <xsd:annotation>
                      <xsd:documentation>This is the option for recovering flights that were created from the provided time to present.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="timeRange">
                    <xsd:annotation>
                      <xsd:documentation>This is option for recovering flights created during a specific time range.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="startTime" type="xsd:dateTime">
                          <xsd:annotation>
                            <xsd:documentation>The starting time of the time range.</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="endTime" type="xsd:dateTime">
                          <xsd:annotation>
                            <xsd:documentation>The ending time of the time range.</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:choice>
              </xsd:complexType>
            </xsd:element>
            <xsd:element name="airportSelect" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Optional airport selection. If present only flights from the specified airports are selected.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="select">
                    <xsd:annotation>
                      <xsd:documentation>Allows selection of arrivals, departures, or both.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="arrivals"/>
                        <xsd:enumeration value="departures"/>
                        <xsd:enumeration value="both"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="airport" type="nce:airportIdType" minOccurs="1" maxOccurs="10">
                    <xsd:annotation>
                      <xsd:documentation>Airport identifier.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="byFltRef">
        <xsd:annotation>
          <xsd:documentation>Request by flight reference number list of up to 128 flights.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence minOccurs="1" maxOccurs="128">
            <xsd:element name="flight" type="fce:idfrType">
              <xsd:annotation>
                <xsd:documentation>Flight reference numbers.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="stopReconstitution">
        <xsd:annotation>
          <xsd:documentation>This message is used to stop an Active or Queued Reconstitution Request.  Stop Reconstitution is not currently supported in R13, but may be supported in a future release.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="reconstitutionUniqueMsgId" type="nce:uuidType">
              <xsd:annotation>
                <xsd:documentation>The uniqueMsgId specified by the user for the original reconstitution request that is meant to be stopped.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="flightReconDataType">
    <xsd:annotation>
      <xsd:documentation>Defines the data returned by a flight resync request.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="transmitTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Time data was transmitted.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="seqNumber" type="xsd:positiveInteger">
        <xsd:annotation>
          <xsd:documentation>Provides the flight block sequence number.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="maxSeqNumber" type="xsd:positiveInteger">
        <xsd:annotation>
          <xsd:documentation>The sequence number of the last resopnse message for this reconstitution.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="last" type="xsd:boolean">
        <xsd:annotation>
          <xsd:documentation>If true it indicates the last block in the sequence.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="flightData" type="flightReconstitutionInformationType" maxOccurs="128">
        <xsd:annotation>
          <xsd:documentation>The flight data.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="flightScheduleReqType">
    <xsd:annotation>
      <xsd:documentation>Defines the possible requests associated with manipulation of the FLIGHT Schedule database.</xsd:documentation>
    </xsd:annotation>
    <xsd:choice>
      <xsd:element name="cancelFS">
        <xsd:annotation>
          <xsd:documentation>The Cancel command cancels the current day's flight plan for a specified flight number in the TFMS flight schedule database. In this context, current day is the period beginning 15 hours before the current time and ending 15 hours afterward. Also, the flight plan must be for a scheduled (that is, not active) flight.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="flightId" type="nce:aircraftIdType">
              <xsd:annotation>
                <xsd:documentation>Aircraft identifier or ACID.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="departureAirport" type="nce:airportIdType">
              <xsd:annotation>
                <xsd:documentation>Flight's departure airport.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="departureTime" type="fm2:hhmmTimeType">
              <xsd:annotation>
                <xsd:documentation>Departure time.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="destinationAirport" type="nce:airportIdType">
              <xsd:annotation>
                <xsd:documentation>Destination airport.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="activateFS" type="activateCommandType">
        <xsd:annotation>
          <xsd:documentation>The Activate command activates a flight or airline in the TFMS airline schedule database, thus removing a restriction that was previously set with the inhibit command.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="inhibitFS" type="inhibitCommandType">
        <xsd:annotation>
          <xsd:documentation>The inhibit command inhibits either a specific flight or all flights for a specific airline in the TFMS flight schedule database.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="removeFS" type="removeRestoreCommandType">
        <xsd:annotation>
          <xsd:documentation>The Remove command lets you remove flights from the TFMS database, as though they had never existed. Using this command you can remove an airline that goes on strike, eliminate duplicate flight numbers, or remove a specific incorrect flight.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="restoreFS" type="removeRestoreCommandType">
        <xsd:annotation>
          <xsd:documentation>The Restore command lets you "undo" the effects of the Remove command. It allows some or all of the flights that you did not want to remove from the database to be restored.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="updateFS">
        <xsd:annotation>
          <xsd:documentation>The TFMS Flight Plan Schedule Database (FPSD) command allows the traffic management specialist to add a flight to the TFMS airline schedule database or to edit a flight plan that already exists in the database.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="flightId" type="nce:aircraftIdType">
              <xsd:annotation>
                <xsd:documentation>Flight identifier or ACID.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="departureAirport" type="nce:airportIdType">
              <xsd:annotation>
                <xsd:documentation>Departure airport.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="departureTime" type="fm2:hhmmTimeType">
              <xsd:annotation>
                <xsd:documentation>Departure time.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="destinationAirport" type="nce:airportIdType">
              <xsd:annotation>
                <xsd:documentation>Destination airport.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="estimatedTimeInRoute" type="eteType">
              <xsd:annotation>
                <xsd:documentation>Estimate time in route.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="aircraftType" type="nce:typeOfAircraftType">
              <xsd:annotation>
                <xsd:documentation>Type of aircraft.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:sequence minOccurs="0">
              <xsd:element name="daysOfOperation" type="fm2:daysOfOperationType" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Days of operation.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="startDate" type="xsd:date" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Starting date.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="stopDate" type="xsd:date" minOccurs="0">
                <xsd:annotation>
                  <xsd:documentation>Ending date.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="fxaReqType">
    <xsd:annotation>
      <xsd:documentation>Type of FEA/FCA requests.</xsd:documentation>
    </xsd:annotation>
    <xsd:choice>
      <xsd:element name="fcaUpdateRequestMessage" type="fcaUpdateMessageType">
        <xsd:annotation>
          <xsd:documentation>FEA/FCA update request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="fcaDeleteRequestMessage" type="fxaDeleteType">
        <xsd:annotation>
          <xsd:documentation>FEA/FCA delete request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="deleteSecondaryFiltersRequestMessage" type="fm2:secondaryFilterDelType" maxOccurs="100">
        <xsd:annotation>
          <xsd:documentation>FEA/FCA PUBLIC secondary filter delete request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>
  <xsd:simpleType name="GUFIType">
    <xsd:annotation>
      <xsd:documentation>Global Unique Flight Identifier is a 10-character alphanumeric identifier. The first character represents an ICAO country code. The second character is a single-letter facility ID. The last eight characters are a unique identifier for each flight.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:length value="10"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="progressStatusType">
    <xsd:annotation>
      <xsd:documentation>Enumerated progress status type.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="SUCCESS"/>
      <xsd:enumeration value="ERROR"/>
      <xsd:enumeration value="CRITICAL_ERROR"/>
      <xsd:enumeration value="COMPLETE"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="rerouteReplyType">
    <xsd:annotation>
      <xsd:documentation>The acknowledgement will contain a response for a user request. The response can be either an error response or a confirmation response. This response will be formatted as a string field. The acknowledgement field can occur more than once for error responses. If there is more than one field that has failed syntax checks, each failed field will be sent back to the user.</xsd:documentation>
    </xsd:annotation>
    <xsd:choice>
      <xsd:element name="waypointReply">
        <xsd:annotation>
          <xsd:documentation>Waypoint reply data.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="routeText" type="xsd:string"/>
            <xsd:element name="waypoint" type="xsd:string" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>latitude, longitude, name, type</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="mergeReplyData">
        <xsd:annotation>
          <xsd:documentation>Merge reply data.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="ctrMergedRoute" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>The merged routes.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="flight" type="fcm:fltIdType">
                    <xsd:annotation>
                      <xsd:documentation>Flight identification data.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="gufi" type="GUFIType">
                    <xsd:annotation>
                      <xsd:documentation>Global Unique Flight Identification.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="mergedRteText" type="rerouteRouteType" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Merged route text definition.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="protectedSegment" type="fce:protectedSegmentType" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Protected segment.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="rrConformance" type="fm2:conformanceStatusType" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Enumerated value representing the conformance status of the merged route. Values include C=Conformant, NC=Non-Conformant, UNKN=Unknown. Note: This field will not be present if an error occurs.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="mergedRteWaypoints" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Field will not be present if an error occurs.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="waypoint" type="xsd:string" maxOccurs="unbounded">
                          <xsd:annotation>
                            <xsd:documentation>latitude, longitude, name, type</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="mergeRouteWarning" minOccurs="0" type="xsd:string"/>
                  <xsd:element name="mergeRouteError" type="xsd:string" minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation>Text of the error message if an error is encountered that prevents the creation of the merged route for this flight. Present only if an error is encountered.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="previewAmendmentReply" type="reroutePreviewAmendmentReplyType">
        <xsd:annotation>
          <xsd:documentation>Preview amendment reply data.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="dynamicExceptionReply" type="rerouteDynamicExceptionResponseType"/>
      <xsd:element name="amendmentReply" type="fm2:rerouteAmendmentResponseType"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="rerouteReqType">
    <xsd:annotation>
      <xsd:documentation>Type of reroute requests.</xsd:documentation>
    </xsd:annotation>
    <xsd:choice>
      <xsd:element name="update" type="rerouteUpdateType">
        <xsd:annotation>
          <xsd:documentation>Update reroute.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="delete" type="rerouteDeleteType">
        <xsd:annotation>
          <xsd:documentation>Delete reroute.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="rrAmendment" type="rerouteAmendmentType">
        <xsd:annotation>
          <xsd:documentation>Reroute amendment.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="rrPreviewAmendment" type="reroutePreviewAmendmentType">
        <xsd:annotation>
          <xsd:documentation>Preview reroute amendment.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="rrMerge" type="rerouteMergeType">
        <xsd:annotation>
          <xsd:documentation>Merge routes.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="rrCancel" type="rerouteCancelType">
        <xsd:annotation>
          <xsd:documentation>Cancel reroutes.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="dynamicException" type="rerouteDynamicExceptionType">
        <xsd:annotation>
          <xsd:documentation>Reroute dynamic exception.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="waypointRequest" type="ambiguousRouteType">
        <xsd:annotation>
          <xsd:documentation>Waypoints request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="responseMessageType">
    <xsd:annotation>
      <xsd:documentation>Defines the error/warning type.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="identifier" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Contextual identifier that can be used to identify an element(s) in the request message that generated this message.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Code" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The optional error/warning code.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="description" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>The error/warning description.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="origMsg" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The original message that caused the error or warning.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="rriaReqType">
    <xsd:annotation>
      <xsd:documentation>Reroute model request types.</xsd:documentation>
    </xsd:annotation>
    <xsd:choice>
      <xsd:element name="rerouteModelRequest" type="rriaFullModelRequestMsgType">
        <xsd:annotation>
          <xsd:documentation>Reroute model request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="rerouteCancelRequest" type="rriaCancelRequestMsgType">
        <xsd:annotation>
          <xsd:documentation>Reroute model cancel request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="rerouteViewRequest" type="rriaViewModelRequestMsgType">
        <xsd:annotation>
          <xsd:documentation>View reroute model results request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="reroutePreviewRequest" type="rriaBaseModelRequestMsgType">
        <xsd:annotation>
          <xsd:documentation>Reroute preview request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="fxaRequest" type="rriaFXARequestMsgType">
        <xsd:annotation>
          <xsd:documentation>FEA/FCA TMI request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="rriaReplyType">
    <xsd:annotation>
      <xsd:documentation>Defines the reply data for Reroute model requests.</xsd:documentation>
    </xsd:annotation>
    <xsd:choice>
      <xsd:element name="rerouteModelReply" type="rriaModelReplyMsgType">
        <xsd:annotation>
          <xsd:documentation>Reroute model reply.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="rerouteCancelModelReply" type="rriaCancelReplyMsgType">
        <xsd:annotation>
          <xsd:documentation>Reroute Cancel model reply.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="reroutePreviewReply" type="rriaPreviewReplyMsgType">
        <xsd:annotation>
          <xsd:documentation>Reroute model preview reply.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="tmiListReply" type="rriaTMIListReplyMsgType">
        <xsd:annotation>
          <xsd:documentation>Reroute model TMI list reply.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="fxaRRModelReply" type="rriaFCAFlightsDataType">
        <xsd:annotation>
          <xsd:documentation>Reroute model FEA/FCA reply.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="tmiResyncReqType">
    <xsd:annotation>
      <xsd:documentation>Type definition for requests for a list of ACTIVE and SUSPENDED TMI identifiers for the purpose of selecting TMIs to resynchronize; only TMIs visible to the users are provided.</xsd:documentation>
    </xsd:annotation>
    <xsd:choice>
      <xsd:element name="requestTmiList">
        <xsd:annotation>
          <xsd:documentation>This is a request a list of TMI identifiers visible to the user.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="ctop" type="xsd:boolean">
              <xsd:annotation>
                <xsd:documentation>If true CTOPs are included.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="fxa" type="xsd:boolean">
              <xsd:annotation>
                <xsd:documentation>If true FEA/FCAs are included.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="reroute" type="xsd:boolean">
              <xsd:annotation>
                <xsd:documentation>If true Reroutes are included.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="afp" type="xsd:boolean">
              <xsd:annotation>
                <xsd:documentation>If true AFPs are included.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="gdp" type="xsd:boolean">
              <xsd:annotation>
                <xsd:documentation>If true GDPs are included.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="gs" type="xsd:boolean">
              <xsd:annotation>
                <xsd:documentation>If true GS are included.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="compression" type="xsd:boolean">
              <xsd:annotation>
                <xsd:documentation>If true COMPRESSION are included.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="blanket" type="xsd:boolean">
              <xsd:annotation>
                <xsd:documentation>If true BLANKET are included.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="requestTmiDef">
        <xsd:annotation>
          <xsd:documentation>Provides the Request TMI definition.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="tmiReferenceNumber" type="fce:idfrType">
              <xsd:annotation>
                <xsd:documentation>The TFMS TMI Reference number unique to TMI type.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="tmiType" type="fm2:tmiTypeType">
              <xsd:annotation>
                <xsd:documentation>The TMI type.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="includeFlightList" type="xsd:boolean">
              <xsd:annotation>
                <xsd:documentation>If true and applicable to the request the last updated flight list is included.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="includeFxaSecondaryFilters" type="xsd:boolean" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>If true and applicable to the request the secondary filters visible to the user are included. NOTE: This field is only considered when the TMI requested is an FXA.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="edctModelResponseType">
    <xsd:annotation>
      <xsd:documentation>Defines the EDCT model response time.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="limitedControlTimes">
        <xsd:annotation>
          <xsd:documentation>Provides the limited control times.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:attributeGroup ref="controlTimePairType">
            <xsd:annotation>
              <xsd:documentation>Limited control times.</xsd:documentation>
            </xsd:annotation>
          </xsd:attributeGroup>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="unlimitedControlTimes">
        <xsd:annotation>
          <xsd:documentation>Provides the unlimited control times.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:attributeGroup ref="controlTimePairType">
            <xsd:annotation>
              <xsd:documentation>Unlimited control times.</xsd:documentation>
            </xsd:annotation>
          </xsd:attributeGroup>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="overridePreviewResponseType">
    <xsd:annotation>
      <xsd:documentation>Defines the override preview response type.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="waypointList" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>List of the LATs and LONGs of every waypoint in the flight's current route. At least two waypoints must be given. Units are decimal degrees. A comma between the LAT and the LONG, and a space between each waypoint value pair.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="edctRequired" type="xsd:boolean" use="required">
      <xsd:annotation>
        <xsd:documentation>EDCT required indicator.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="ctopReplyType">
    <xsd:annotation>
      <xsd:documentation>Defines a CTOP reply type.</xsd:documentation>
    </xsd:annotation>
    <xsd:choice>
      <xsd:element name="CTOP_IDFL">
        <xsd:annotation>
          <xsd:documentation>The CTOP flight list.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:all>
            <xsd:element name="IDFL_UPDATE_TIME" type="xsd:dateTime">
              <xsd:annotation>
                <xsd:documentation>Time CTOP list was last updated.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="CTOP_NAME" type="fcm:ctopNameType">
              <xsd:annotation>
                <xsd:documentation>Name for the CTOP as defined by the Traffic Manager.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="CTOP_SHORTNAME" minOccurs="0" type="fcm:ctopTmiIdType">
              <xsd:annotation>
                <xsd:documentation>CTOP short name.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="CTOP_UNIQUE_ID" type="fm2:ctopUniqueIdType">
              <xsd:annotation>
                <xsd:documentation>CTOP unique identifier.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="VERSION" type="xsd:int" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>CTOP version.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="CTOP_IDFR" type="fce:idfrType">
              <xsd:annotation>
                <xsd:documentation>CTOP reference number.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="CTOP_STATUS" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Current status of selected CTOP.</xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="ACTIVE"/>
                  <xsd:enumeration value="MODEL"/>
                  <xsd:enumeration value="PROPOSED"/>
                  <xsd:enumeration value="CANCELLED"/>
                  <xsd:enumeration value="EXPIRED"/>
                  <xsd:enumeration value="SUSPENDED"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="ALLOCATION_TIME" type="xsd:dateTime" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Latest model time for selected CTOP.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="CTOP_LAST_UPDATED" type="xsd:dateTime">
              <xsd:annotation>
                <xsd:documentation>Last update time for selected CTOP.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="CTOP_FLIGHT_COUNT" type="xsd:int">
              <xsd:annotation>
                <xsd:documentation>CTOP flight count.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="DISPLAY_LEGACY_CONFORMANCE_STATUS" type="xsd:boolean">
              <xsd:annotation>
                <xsd:documentation>Indicator to display legacy conformance status.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="CTOP_FCA_LIST">
              <xsd:annotation>
                <xsd:documentation>Container for the list of FCAs that are part of this CTOP.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="FCA" type="fcaIdType" maxOccurs="4">
                    <xsd:annotation>
                      <xsd:documentation>System-generated unique identifier of the FCA</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element name="MODEL_STATISTICS" type="modelStatistics" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>An optional set of statistics provided when modeling a CTOP.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="FLIGHT_LIST" type="idflFlightListType" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>Container for the flight list.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="FCA_IDFL">
        <xsd:annotation>
          <xsd:documentation>The FEA/FCA flight list.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:all>
            <xsd:element name="IDFL_UPDATE_TIME" type="xsd:dateTime">
              <xsd:annotation>
                <xsd:documentation>FCA last flight update time.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="FCA_LIST">
              <xsd:annotation>
                <xsd:documentation>Container for the list of FCAs that are part of this CTOP.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="FCA" type="fcaIdType" maxOccurs="4">
                    <xsd:annotation>
                      <xsd:documentation>FCA identifier.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element name="FLIGHT_LIST" type="idflFlightListType">
              <xsd:annotation>
                <xsd:documentation>Container for the flight list.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ctopProcessingStatusUpdate" type="ctopProcessingStatusUpdateType">
        <xsd:annotation>
          <xsd:documentation>CTOP Processing Status Update.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="edctModelResponse" type="edctModelResponseType">
        <xsd:annotation>
          <xsd:documentation>EDCT model response.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="overridePreviewResponse" type="overridePreviewResponseType">
        <xsd:annotation>
          <xsd:documentation>CTOP override preview response.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="flightHistory">
        <xsd:annotation>
          <xsd:documentation>CTOP flight history.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="columnHeaders" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>CTOP flight history: Comma delimited list of column title.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="columnValues" type="xsd:string" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>CTOP flight history: Comma delimited list of column values.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="edctAcCommandType">
    <xsd:annotation>
      <xsd:documentation>EDCT AC ON/OFF Command (Adaptive Compression)</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="switch" type="fce:onOffType">
        <xsd:annotation>
          <xsd:documentation>ON/OFF indicator.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="controlElement" type="fce:controlElementNameType">
        <xsd:annotation>
          <xsd:documentation>Control element name.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="edctBridgingToggleCommandType">
    <xsd:annotation>
      <xsd:documentation>The Bridging Off message temporarily disables flights for the sending airline from being used as part of an SCS bridge in a particular GDP. The Bridging On message restores the use of flights for the sending airline from being used as part of an SCS bridge in a particular GDP.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="switch" type="fce:onOffType">
        <xsd:annotation>
          <xsd:documentation>Switch on/off status.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="controlElement" type="fce:controlElementNameType">
        <xsd:annotation>
          <xsd:documentation>Control element.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="edctCheckCommandType">
    <xsd:annotation>
      <xsd:documentation>The EDCT Check command displays the current status of a single flight controlled by an EDCT program.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="aircraft" type="nce:aircraftIdType">
        <xsd:annotation>
          <xsd:documentation>Aircraft call sign/identifier.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="origin" type="nce:airportIdType">
        <xsd:annotation>
          <xsd:documentation>Departing airport.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="destination" type="nce:airportIdType">
        <xsd:annotation>
          <xsd:documentation>Arrival airport.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="edctCommandRequestType">
    <xsd:annotation>
      <xsd:documentation>Wrapper type for ALL incoming EDCT Commands and Reports</xsd:documentation>
    </xsd:annotation>
    <xsd:choice>
      <xsd:element name="adaptiveCompression" type="edctAcCommandType">
        <xsd:annotation>
          <xsd:documentation>Adaptive compression request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="bridging" type="edctBridgingToggleCommandType">
        <xsd:annotation>
          <xsd:documentation>Bridging request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="check" type="edctCheckCommandType">
        <xsd:annotation>
          <xsd:documentation>Check request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="hold" type="edctHoldCommandType">
        <xsd:annotation>
          <xsd:documentation>EDCT hold request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="list" type="edctListCommandType">
        <xsd:annotation>
          <xsd:documentation>List request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="log" type="edctLogCommandType">
        <xsd:annotation>
          <xsd:documentation>EDCT log  request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="purge" type="edctPurgeCommandType">
        <xsd:annotation>
          <xsd:documentation>Purge request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="release" type="edctReleaseCommandType">
        <xsd:annotation>
          <xsd:documentation>Release request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="remove" type="edctRemoveRestoreCommandType">
        <xsd:annotation>
          <xsd:documentation>Remove request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="restore" type="edctRemoveRestoreCommandType">
        <xsd:annotation>
          <xsd:documentation>Restore request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="scsToggle" type="edctScsToggleCommandType">
        <xsd:annotation>
          <xsd:documentation>SCS toggle request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="show" type="edctShowCommandType">
        <xsd:annotation>
          <xsd:documentation>Show command.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="subShow" type="edctSubShowCommandType">
        <xsd:annotation>
          <xsd:documentation>Substitution show request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="subToggle" type="edctSubToggleCommandType">
        <xsd:annotation>
          <xsd:documentation>Substitute toggle request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="slist" type="edctSlistCommandType">
        <xsd:annotation>
          <xsd:documentation>Slot list request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="update" type="edctUpdateCommandType">
        <xsd:annotation>
          <xsd:documentation>Update request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="unassignedSlots" type="edctUnassignedSlotsCommandType">
        <xsd:annotation>
          <xsd:documentation>Unassigned slot request.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="edctHoldCommandType">
    <xsd:annotation>
      <xsd:documentation>The EDCT Hold command holds the slots of all flights in a GDP or an AFP, or only the slots of flights belonging to a specified airline and its sub-carriers.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="controlElement" type="fce:controlElementNameType">
        <xsd:annotation>
          <xsd:documentation>Control element.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="airline" type="nce:airlineIdType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Air carrier.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="edctListCommandType">
    <xsd:annotation>
      <xsd:documentation>The EDCT List command creates a report containing a list of all airports currently controlled by EDCT programs.</xsd:documentation>
    </xsd:annotation>
  </xsd:complexType>
  <xsd:complexType name="edctLogCommandType">
    <xsd:annotation>
      <xsd:documentation>The EDCT Log command creates a report containing all EDCT Log information for the requested date.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="date" type="xsd:date">
        <xsd:annotation>
          <xsd:documentation>Date the Log report will be generated for.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="edctPurgeCommandType">
    <xsd:annotation>
      <xsd:documentation>The EDCT Purge command purges EDCT flight controls for a specified airport or FCA or for all airports or FCAs.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="controlElement" type="fce:controlElementNameType">
        <xsd:annotation>
          <xsd:documentation>Control element.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="time" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Date and time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="directionality" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Direction: BEFORE or AFTER.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="BEFORE"/>
            <xsd:enumeration value="AFTER"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="edctReleaseCommandType">
    <xsd:annotation>
      <xsd:documentation>The EDCT Release command releases all slots in a GDP or AFP or only the slots of flights belonging to a specified airline and its sub-carriers.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="controlElement" type="fce:controlElementNameType">
        <xsd:annotation>
          <xsd:documentation>Control element.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="airline" type="nce:airlineIdType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Air carrier.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="edctRemoveRestoreCommandType">
    <xsd:annotation>
      <xsd:documentation>The EDCT Remove command will remove flights from the TFMS database, as if they never existed; the EDCT Restore command will undo the effects of the Remove command.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:choice>
        <xsd:element name="airlineId" type="nce:airlineIdType">
          <xsd:annotation>
            <xsd:documentation>Three-letter airline code. Either Airline or Flight is required, but not both.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="flightId" type="nce:aircraftIdType" maxOccurs="20">
          <xsd:annotation>
            <xsd:documentation>Aircraft call sign or identifier.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="flightIdRange" type="flightIdRangeType">
          <xsd:annotation>
            <xsd:documentation>A full flight call sign. Either Airline or Flight is required. Can have a single value, a range of values, or a list of values.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:choice>
      <xsd:choice>
        <xsd:element name="date" type="xsd:date" maxOccurs="20">
          <xsd:annotation>
            <xsd:documentation>A date in month/day in mmdd format. This is a required value. Can have a single value, a range of values, or a list of values.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="dateRange" type="fce:dateRangeType">
          <xsd:annotation>
            <xsd:documentation>A date in month/day in mmdd format. This is a required value. Can have a single value, a range of values, or a list of values.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="ALL">
          <xsd:annotation>
            <xsd:documentation>ALL in this field to indicates all dates.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:choice>
      <xsd:element name="time" minOccurs="0" type="fm2:hhmmTimeType">
        <xsd:annotation>
          <xsd:documentation>Optional time value. Format is HHMM.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:choice>
        <xsd:element name="airport" type="nce:airportIdType" minOccurs="0" maxOccurs="20">
          <xsd:annotation>
            <xsd:documentation>A three or four-character airport code. Optional entry. The absence of an airport entry indicates all airports.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="airportLists" minOccurs="0" maxOccurs="1">
          <xsd:annotation>
            <xsd:documentation>Optional departure and arrival airport entry; the absence of an airport entry indicates all airports.</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="origin" type="nce:airportIdType" maxOccurs="20" minOccurs="0"/>
              <xsd:element name="destination" type="nce:airportIdType" maxOccurs="20" minOccurs="0"/>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
      <xsd:element name="origin" type="fm2:originationType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Flights that have only OAG or CDM messages.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="edctScsToggleCommandType">
    <xsd:annotation>
