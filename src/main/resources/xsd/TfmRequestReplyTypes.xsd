        <xsd:annotation>
          <xsd:documentation>Reroute Identification.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="lastUpdateTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Last update time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
