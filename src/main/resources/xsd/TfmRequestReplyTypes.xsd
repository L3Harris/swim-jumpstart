      <xsd:element name="modelUpdateTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Model update time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="fxaId" type="rriaIdType">
        <xsd:annotation>
          <xsd:documentation>FXA identification.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="rriaModelReplyMsgType">
