  <xsd:complexType name="reqCtopTmiType">
    <xsd:annotation>
      <xsd:documentation>
    Grouping of CTOP TMI xml elements for defining a common type used by the TSD
    (differs from FOS)
   </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="reqCtopTmiModelType">
        <xsd:sequence>
          <xsd:element name="ADVISORY_TEXT" minOccurs="1">
            <xsd:annotation>
              <xsd:documentation>
      The advisory text. Optional because initially created CTOPs that have not
      yet been proposed or activated do not require advisories to be distributed.
     </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:maxLength value="4000"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="ADVISORY_NUMBER" type="advisoryNumType" minOccurs="1">
            <xsd:annotation>
              <xsd:documentation>
      The advisory's unique number. Optional because initially created CTOPs
      that have not yet been proposed or activated do not require advisories to
      be distributed.
     </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="REMARKS" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>
      A free text description of the CTOP TMI.
     </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:maxLength value="4000"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="ROUTE_GUIDANCE" minOccurs="0">
            <xsd:annotation>
              <xsd:documentation>
      The free form text optionally supplied with the CTOP advisory. Meant to
      serve as a suggestion from the traffic manager and optionally displayed in
      the reroute Monitor.
     </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:maxLength value="4000"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="CONSTRAINED_AREA" minOccurs="1">
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:maxLength value="1000"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="CHARGE_TO_FACILITY_TYPE" minOccurs="1">
            <xsd:annotation>
              <xsd:documentation>
      Indicates the type of facility to which the delay is charged 
      (e.g. Center, Terminal, etc.).
     </xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:enumeration value="CENTER"/>
                <xsd:enumeration value="TERMINAL"/>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:element>
          <xsd:element name="CHARGE_TO_FACILITY" type="xsd:string" minOccurs="1">
            <xsd:annotation>
              <xsd:documentation>
      Indicates the facility to which delay is charged (e.g. N90, ZNY, etc.).
     </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="IMPACTING_CONDITION_CODE" type="fce:impactingConditionCodeType" minOccurs="1">
            <xsd:annotation>
              <xsd:documentation>
      Indicates the OPSNET numerical code for the Impacting Condition selected.
     </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
          <xsd:element name="IMPACTING_CONDITION_EQUIPMENT" minOccurs="0">
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element name="FAA" type="xsd:boolean"/>
                <xsd:element name="scheduled" type="xsd:boolean"/>
              </xsd:sequence>
            </xsd:complexType>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="reqCtopTmiModelType">
    <xsd:annotation>
      <xsd:documentation>
    Grouping of CTOP TMI xml elements for defining a common type used by the TSD
    (differs from FOS)
   </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="CTOP_NAME" type="fcm:ctopNameType">
        <xsd:annotation>
          <xsd:documentation>
      Name as defined by the Traffic Manager
     </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CTOP_SHORT_NAME" type="ctopTmiShortNameType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
      System generated short name. Has the format: CTPddd.
     </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="VERSION" type="xsd:int" minOccurs="0"/>
      <xsd:element name="CTOP_STATUS">
        <xsd:annotation>
          <xsd:documentation>
        Current status of selected CTOP.
      </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="ACTIVE"/>
            <xsd:enumeration value="MODEL"/>
            <xsd:enumeration value="PROPOSED"/>
            <xsd:enumeration value="CANCELLED"/>
            <xsd:enumeration value="EXPIRED"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="CTOP_UNIQUE_ID" type="fm2:ctopUniqueIdType">
        <xsd:annotation>
          <xsd:documentation>
      The system generated unique identifier for the CTOP TMI. Has the following
      format:
      {ctoptmi}{.}&lt;site&gt;{.}&lt;workstation&gt;{.}&lt;creationdate/time&gt;
     </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CTOP_RANK" type="fcm:ctopRankType">
        <xsd:annotation>
          <xsd:documentation>
      Ranking indicating the precedence level of this CTOP relative to other
      CTOPs.
     </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PROGRAM_START_TIME" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>The start of the earliest controlled time bin of this
                        CTOP.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PROGRAM_END_TIME" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>The end of the latest controlled time bin of this CTOP.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CUMULATIVE_PROGRAM_START_TIME" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>The start of the earliest controlled time bin of any
                        revision of this CTOP.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CUMULATIVE_PROGRAM_END_TIME" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>The end of the latest controlled time bin of any
                        revision of this CTOP.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="EXEMPT_CRITERIA" type="fcm:ctopExemptCriteriaType">
        <xsd:annotation>
          <xsd:documentation>
      Container for the exemption criteria.
     </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="POPUP_DELAY_LIMIT">
        <xsd:annotation>
          <xsd:documentation>
      When a flight first pops up in a CTOP, TFMS will attempt to find an
      available slot for the flight without exceeding the popup delay limit, and
      otherwise will assign the delay limit amount of delay to the flight. The
      popup delay limit is expressed in minutes.
     </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:integer">
            <xsd:minInclusive value="0"/>
            <xsd:maxInclusive value="999"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="FCA_LIST">
        <xsd:annotation>
          <xsd:documentation>
      Container for the FCAs that are part of this CTOP.
     </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:annotation>
              <xsd:documentation/>
            </xsd:annotation>
            <xsd:element name="FCA" maxOccurs="4">
              <xsd:annotation>
                <xsd:documentation>
         Container for an FCA
        </xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:all>
                  <xsd:element name="FCA_ID" type="fcm:simpleFcaIdType">
                    <xsd:annotation>
                      <xsd:documentation>
                          System generated unique FCA identifier.
                      </xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="FCA_BIN_LIST">
                    <xsd:annotation>
                      <xsd:documentation>
                          Container added for the grouping of CTOP FCA time bins.
                      </xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="FCA_BIN" type="fcm:fxaBinType" maxOccurs="unbounded">
                          <xsd:annotation>
                            <xsd:documentation>
                                Container for a time bin for the FCA that is part of the CTOP.
                            </xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:all>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="MANUAL_OVERRIDES" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
      Provides capability to manually override trajectories for one or more
      flights.
     </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="FLIGHT" type="ctopManualOverrideType" maxOccurs="unbounded">
              <xsd:annotation>
                <xsd:documentation>
         Information needed to override a flight manually.
        </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="AUTO_REVISION">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="AUTO_REVISION_ENABLED">
              <xsd:annotation>
                <xsd:documentation>
         Defines whether automatic revision is enabled for the CTOP.
        </xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:boolean"></xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="REFRESH_INTERVAL" type="fcm:refreshIntervalType" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>
         Defines how often TFMS checks the FCA bin triggers. Value is required
         when AUTO_REVISION is TRUE. In minutes
        </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="SMOOTHING_FACTOR" minOccurs="0">
              <xsd:annotation>
                <xsd:documentation>
         Defines the number of time bins over which the CTOP algorithm should
         smooth.
        </xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:integer">
                  <xsd:minInclusive value="1"/>
                  <xsd:maxInclusive value="8"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="AUTO_REVISION_PERIOD" minOccurs="0">
              <xsd:complexType>
                <xsd:choice>
                  <xsd:element name="FIXED_TIMES" minOccurs="1">
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="AR_FIRST_TIME_BIN" type="xsd:dateTime" minOccurs="1">
                          <xsd:annotation>
                            <xsd:documentation>
         The start time of the first time bin to which auto-revision will be
         applied. Present only if AUTO-REVISION is TRUE.
        </xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                        <xsd:element name="AR_LAST_TIME_BIN" type="xsd:dateTime" minOccurs="1">
                          <xsd:annotation>
                            <xsd:documentation>
         The start time of the last time bin to which auto-revision will be
         applied. Present only if AUTO-REVISION is TRUE.
        </xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="ROLLING" minOccurs="1">
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="LOOK_AHEAD">
                          <xsd:simpleType>
                            <xsd:restriction base="xsd:integer">
                              <xsd:minInclusive value="1"/>
                              <xsd:maxInclusive value="10"/>
                            </xsd:restriction>
                          </xsd:simpleType>
                        </xsd:element>
                        <xsd:element name="OFFSET">
                          <xsd:simpleType>
                            <xsd:restriction base="xsd:integer">
                              <xsd:minInclusive value="0"/>
                              <xsd:maxInclusive value="3"/>
                            </xsd:restriction>
                          </xsd:simpleType>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:choice>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="CREATION_DATETIME" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>
      The date and time the CTOP TMI was created.
     </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CREATION_WORKSTATION">
        <xsd:annotation>
          <xsd:documentation>
      The workstation that created the CTOP TMI.
     </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="8"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="CREATION_SITE">
        <xsd:annotation>
          <xsd:documentation>
      The site that created the CTOP TMI.
     </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:maxLength value="8"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="UPDATE_WORKSTATION" type="xsd:string" minOccurs="0"/>
      <xsd:element name="UPDATE_SITE" type="xsd:string" minOccurs="0"/>
      <xsd:element name="MANUAL_REVISION" type="xsd:boolean">
        <xsd:annotation>
          <xsd:documentation>
      True if the user decides that with an incoming edit the CTOP will also be
      revised (full allocation will be run).
     </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="LAST_UPDATED" type="xsd:dateTime"/>
      <xsd:element name="RANK_PROMOTED" type="xsd:boolean" minOccurs="0"/>
      <xsd:element name="CURRENT_RANK" type="fcm:ctopRankType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="flightReconstitutionInformationType">
    <xsd:annotation>
      <xsd:documentation>The purpose of the Flight Reconstitution Type Information message is to provide all known flight parameters for the purpose of establishing a baseline state for the flight.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="flightReference" type="fce:idfrType">
        <xsd:annotation>
          <xsd:documentation>Defines the flight reference that can be used to correlate flight data.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="qualifiedAircraftId" type="nce:qualifiedAircraftIdType">
        <xsd:annotation>
          <xsd:documentation>Qualified Aircraft Identification of the flight this flight management information applies to.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="flightStatus" type="fm2:flightStateType">
        <xsd:annotation>
          <xsd:documentation>Aircraft model e.g. B737.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="aircraftModel" type="nce:typeOfAircraftType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Aircraft model e.g. B737.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="aircraftspecification" type="nce:flightAircraftSpecificationType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Aircraft information: Number of aircraft, aircraft type, and use class type.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="beaconCode" type="nce:beaconCodeType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Beacon Code of the flight this flight plan information refers to. This field is optional and will only be present when the data is available.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:sequence minOccurs="0">
        <xsd:element name="coordinationPoint" type="nce:fixType">
          <xsd:annotation>
            <xsd:documentation>Coordination point of the flight this flight plan information refers to with respect to the facility coordinating the flight If the flight traverses multiple facilities, each facility may have a different coordination point.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="coordinationTime">
          <xsd:annotation>
            <xsd:documentation>Coordination time of the flight this flight plan information refers to with respect to the facility coordinating the flight If the flight traverses multiple facilities, each facility will have a different coordination time.</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:simpleContent>
              <xsd:extension base="nce:coordinationTimeType"/>
            </xsd:simpleContent>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
      <xsd:element name="altitude" type="nce:reportedAltitudeType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Flight altitude and type.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="speed" type="nce:flightPlanSpeedType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Flight speed and type.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="diversionIndicator" type="nce:diversionIndicatorType">
        <xsd:annotation>
          <xsd:documentation>Flight's diversion state.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="rvsmData" type="nce:rvsmComplianceType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>RVSM Compliance status: Equipped, current compliance, and future Compliance.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:sequence minOccurs="0">
        <xsd:element name="dp" type="nce:adaptedRoutNameType">
          <xsd:annotation>
            <xsd:documentation>Departure Procedure name and type.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="dpTransitionFix" type="nce:fixNameType">
          <xsd:annotation>
            <xsd:documentation>Departure Procedure transition fix name.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:sequence minOccurs="0">
        <xsd:element name="star" type="nce:adaptedRoutNameType">
          <xsd:annotation>
            <xsd:documentation>STAR Procedure name and type.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="starTransitionFix" type="nce:fixNameType">
          <xsd:annotation>
            <xsd:documentation>STAR Procedure transition fix name.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:element name="arrivalFixAndTime" type="nce:fixEventType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Arrival fix and associated time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="departureFixAndTime" type="nce:fixEventType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Departure fix and associated time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="nextPosition" type="nce:positionType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Position of next event.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="flightTraversalData2" type="nce:flightTraversalDataType2">
        <xsd:annotation>
          <xsd:documentation>This form of the flightTraversalData element groups the data more like the original legacy message. Waypoints, sectors, fixes, airways, and centers are ORDERED LISTS within their parent elements.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="routeOfFlight" type="nce:field10FormatType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>This element contains the route of flight as filed or amended, i.e. copy of field 10. It is not the converted route.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="remarksKeywords" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>This element contains a comma delimited list of Keywords found in field 11: NRP, LIFEGUARD, CATIII, ALTRV, SWAP, DVRSN, DIVERT, ADCUS, FCA, WXRTE, HAR, PTP.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:sequence minOccurs="0">
        <xsd:element name="plannedPositionData" minOccurs="0" maxOccurs="2">
          <xsd:annotation>
            <xsd:documentation>Planned Position Data is a container for all related Planned Position Data for the flight associated with this Oceanic Report Type.</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:complexContent>
              <xsd:extension base="nce:oceanicPositionDataType">
                <xsd:attribute name="planNumber" type="xsd:integer">
                  <xsd:annotation>
                    <xsd:documentation>Planned position number.</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:extension>
            </xsd:complexContent>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="reportedPositionData" type="nce:oceanicPositionDataType">
          <xsd:annotation>
            <xsd:documentation>Reported Position Data is a container for all related Reported Position Data for the flight associated with this Oceanic Report Type.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:element name="etd" type="nce:etdFlightTimeType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Estimated time of departure and departure type.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="eta" type="nce:etaFlightTimeType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Estimated time of arrival and arrival type.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ctd" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Estimated time of departure and departure type.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="cta" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Estimated time of arrival and arrival type.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="IGTA" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Initial Gate Time of Arrival is the very first initial gate departure time of the flight. IGTA is set when a flight is first created, if it is created from OAG data, CDM Participant data (from CDM message), or flight plan data, the IGTD is set to the gate departure time from that source, thereafter it is never changed. If a flight is created from an active message (e.g., , departure message, airborne flight plan, etc.), the IGTA is not present.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ARTD" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>ARTD is the time from a NAS DZ message. If no DZ has been received, this field is not present.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ARTA" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>ARTA is the time from a NAS AZ message. If no AZ has been received, this field is not present.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SGTD" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>SGTD is Scheduled Gate Time of Departure time from OAG. Not present if no OAG data available for the flight.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SGTA" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>SGTA is Scheduled Gate Time of Arrival time from OAG. Not present if no OAG data available for the flight.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PGTD" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Departure time from NAS flight plan. Null if no flight plan has been received for the flight. If multiple flight plans have been processed, shows the P time from the last one.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PGTA" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Arrival time from NAS. Generally this is initially set from the flight plan, and then updated when AFs, DZs and/or UZs are processed. Not present if no NAS arrival time has been received for the flight.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PETE" type="xsd:positiveInteger" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The PETE field is needed because the PGTD and PGTA pair does not preserve the ETE filed by the flight operator on the flight plan. PETE is set when a NAS flight plan has been received for this flight with an ETE field, PETE is set to the latest flight plan value received. if no flight plan is received the field is not present. PETE is in units of minutes.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="LGTD" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Predicted gate time of departure provided by the CDM Participant in a CDM message. If a CDM Participant has sent in a gate departure time in a CDM message, then this field contains the most recent such time. Otherwise, this field is absent.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="LGTA" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Predicted gate time of arrival provided by the CDM Participant in a CDM message. If a CDM Participant has sent in a gate arrival time in a CDM message, then this field contains the most recent such time. Otherwise, the field is absent.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="LRTD" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Predicted runway time of departure provided by the CDM Participant in a CDM message. If a CDM Participant has sent in a runway departure time in a CDM message, then this field contains the most recent such time. Otherwise, this field is absent.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="LRTA" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Predicted runway time of arrival provided by the CDM Participant in a CDM message. If a CDM Participant has sent in a runway arrival time in a CDM message, then this field contains the most recent such time. Otherwise, the field is absent.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ERTD" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The earliest departure time that the CDM Participant would like to have assigned to this flight in a TMI. If the CDM Participant has sent this field in a CDM FC or FM message, then the most recent such time is contained in this field. Otherwise, this field is absent.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ERTA" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The earliest arrival time that the CDM Participant would like to have assigned to this flight in a TMI. If the CDM Participant has sent this field in a CDM FC or FM message, then the most recent such time is contained in this field. Otherwise, the field is absent.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OUT" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The time at which a flight pushed out from the gate as reported by the CDM Participant via a CDM message. If the CDM Participant sends more than one value, the most recently submitted time is contained in this field. Otherwise, the field is absent.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="IN" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The time at which a flight pulls in at the gate as reported by the CDM Participant via a CDM message. If the CDM Participant sends more than one value, the most recently submitted time is contained in this field. Otherwise, the field is absent.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OFF" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The time at which a flight lifts off from the runway as reported by the CDM Participant via a CDM message. If the CDM participant sends more than one value, the most recently submitted time is contained in this field. Otherwise, the field is absent.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ON" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The time at which a flight touches down on the runway as reported by the CDM Participant via a CDM message. If the CDM participant sends more than one value, the most recently submitted time is contained in this field. Otherwise, the field is absent.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OETD" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>OETD is used to save the ETD last modeled by TFMS-Core before either a TMI is issued, or the flight departs, or the flight is time-out delayed by TFMS-Core. The OETD is used to back out of a TMI. The OETD is different from the BETD in that it does NOT include any time-out delay modeled by TFMS-Core whereas the BETD does include time-out delay.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OETA" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>OETA is used to save the ETA last modeled by TFMS-Core before either a TMI is issued, or the flight departs, or the flight is time-out delayed by TFMS-Core. The OETA is used to back out of a TMI. The OETA is different from the BETA in that it does NOT include any time-out delay modeled by TFMS-Core whereas the BETA does include time-out delay. Whenever the ETA is updated from an FS, FC, FM, or FZ, as long as the flight is not controlled or active, the OETA is updated to the ETA.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BETD" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>BETD is used to save the ETD at the time a TMI is issued or the flight departs. The BETD is used to compute the amount of departure delay that can be attributed to a TMI. The BETD includes any time-out delay modeled by TFMS-Core. BETD is set in the following manner: 1) Whenever an ETD is updated from an FS, FC, FM, or FZ, as long as the flight is not controlled or active, the BETD is set to the new ETD; 2) Whenever CDM re-models a departure time due to a time-out delay, the BETD is updated to the new ETD.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BETA" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>BETA is set in the following manner: 1) Whenever the ETA is updated from an FS, FC, FM, or FZ, as long as the flight is not controlled or active, the BETA is updated to the ETA; 2) Whenever CDM re-models a departure time due to a time-out delay, the BETA is updated to the new ETA.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MSTD" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>TBFM issued scheduled time of departure</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SSOT" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>STDDS surface movement Spot OUT Time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SSIT" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>STDDS surface movement Spot IN Time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SOFT" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>STDDS surface movement OFF Time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SONT" type="xsd:dateTime" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>STDDS surface movement ON Time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CDM_MBR" type="xsd:boolean" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>if true indicates whether this flight belongs to a CDM Participant and is thus eligible for the full benefits of compression.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="MAJOR" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Indicates the organization within which this flight is considered when RBS++ is computed (that is, all flights with the same MAJOR value are considered together during the intra-airline swapping portion of RBS++ and Compression). The MAJOR code can indicate an actual air carrier, a general aviation fleet operator, or a pseudo carrier used to logically group certain flights. If the MAJOR code is three letters, it is an official three-letter code that can be used for flight plan filing. If the MAJOR starts with a period character (.), it is a dummy code used only within TFMS. Dummy codes are used for any organization, such as a GA data provider, that is a CDM Participant but does not have an official three-letter code. If the flight IDs are being filtered, the MAJOR field contains the filtered four-letter code, and the '.' is not indicated.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="GDC" type="xsd:positiveInteger" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Provides the great circle distance, in nautical miles, between the origin and the destination airports.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="edctListControlType">
    <xsd:annotation>
      <xsd:documentation>The control type displayed for the EDCT List report</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="GS"/>
      <xsd:enumeration value="DAS"/>
      <xsd:enumeration value="EDCT"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="fcaFilterIdType">
    <xsd:annotation>
      <xsd:documentation>System generated unique identifier of FEA/FCA secondary filter. Format is filter.{fcaId}.{sitename}.{hostname}.{yyyyMMddhhmmss}</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="filter\.fca\.[A-Za-z0-9]{1,8}\.\w{1,8}\.\d{14}\.[A-Za-z0-9]{1,8}\.\w{1,8}\.\d{14}"/>
      <xsd:maxLength value="100"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="fcaDisplayColumnHeaderType">
    <xsd:annotation>
      <xsd:documentation>Enumerated FCA/FEA flight list column names.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="Abbreviated Route"/>
      <xsd:enumeration value="Aircraft ID"/>
      <xsd:enumeration value="Aircraft Type"/>
      <xsd:enumeration value="Airline"/>
      <xsd:enumeration value="Amendment Status"/>
      <xsd:enumeration value="Arrival Center"/>
      <xsd:enumeration value="Arrival Time"/>
      <xsd:enumeration value="Centers Traversed"/>
      <xsd:enumeration value="Control Element"/>
      <xsd:enumeration value="Controlled Arrival Time"/>
      <xsd:enumeration value="Controlled Departure Time"/>
      <xsd:enumeration value="Controlling Program"/>
      <xsd:enumeration value="Controlling Sector"/>
      <xsd:enumeration value="Current ARTCC"/>
      <xsd:enumeration value="Current Altitude"/>
      <xsd:enumeration value="Departure Center"/>
      <xsd:enumeration value="Departure Time"/>
      <xsd:enumeration value="Destination"/>
      <xsd:enumeration value="Entry Time"/>
      <xsd:enumeration value="Exit Time"/>
      <xsd:enumeration value="FCA Indicator"/>
      <xsd:enumeration value="Filed Speed"/>
      <xsd:enumeration value="Flight Status"/>
      <xsd:enumeration value="Full Route"/>
      <xsd:enumeration value="GUFI"/>
      <xsd:enumeration value="Initial Gate Time of Departure"/>
      <xsd:enumeration value="NRP Indicator"/>
      <xsd:enumeration value="Origin"/>
      <xsd:enumeration value="RVSM"/>
      <xsd:enumeration value="Requested Altitude"/>
      <xsd:enumeration value="Secondary Filter"/>
      <xsd:enumeration value="TMA Release Time"/>
      <xsd:enumeration value="Time to Intersect"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="fcaDefinitionResyncType">
    <xsd:sequence>
      <xsd:element name="FCA" type="fxaDefinitionType"/>
      <xsd:element name="SECONDARY_FILTER" type="fm2:fxaSecondaryFilterType" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="advisoryRequestType">
    <xsd:choice>
      <xsd:element name="advisoryNumber"/>
      <xsd:element name="advisoryDistribution" type="advisoryDistributionRequestType"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="advisoryResponseType">
    <xsd:choice>
      <xsd:element name="advisoryNumberReply" type="advisoryNumType">
        <xsd:annotation>
          <xsd:documentation>Advisory number - will start with 1 daily.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="advisoryDistributionReply" type="advisoryDistributionResponseType"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="tmiIdRequestType">
    <xsd:sequence>
      <xsd:element name="tmiType" type="tmiRequestType">
        <xsd:annotation>
          <xsd:documentation>The type of TMI that needs to retrieve the TMI ID.  Choices are CTOP and REROUTE.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="advisoryNumber" minOccurs="0" type="advisoryNumType">
        <xsd:annotation>
          <xsd:documentation>Advisory number that was previously generated for a public reroute, if one exists.  If no advisory number exists when generating this request, this tag will not be used.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="tmiIdResponseType">
    <xsd:sequence>
      <xsd:element name="tmiId" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>TMI ID - format of this id will be RR&lt;site&gt;&lt;advisoryNbr/tmiIdNbr&gt; - ex.) RRZNY001 this is with an advisory number or RRZNY501 - this is with an TMI ID number.  The numbering taken from the database for TMI ID will start at 500.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="advisoryDistributionRequestType">
    <xsd:sequence>
      <xsd:element name="msgType" type="distributionMsgType">
        <xsd:annotation>
          <xsd:documentation>Type of message - ADVISORY/GENERAL.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="originator" type="originPositionType">
        <xsd:annotation>
          <xsd:documentation>Originator of the Advisory/General message.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="advisoryData" type="advMessageType">
        <xsd:annotation>
          <xsd:documentation>Contents of the Advisory message.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="destination" type="destinationType" maxOccurs="4"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="advisoryDistributionResponseType">
    <xsd:sequence>
      <xsd:element name="distributionResult" type="distributionResultType" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>Distribution results which indicate success/non-succuess for each site that an advisory was distributed to.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="originPositionType">
    <xsd:sequence>
      <xsd:element name="facilityId" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Identifies the site of the distribution request- this is used by NTML.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="positionIpAddr" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>The ip address of where the request came from - this is needed for NTML.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="distributionMsgType">
    <xsd:annotation>
      <xsd:documentation>The type of message.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="ADVISORY"/>
      <xsd:enumeration value="GENERAL"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="distributionResultType">
    <xsd:sequence>
      <xsd:element name="site" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Site id for this distribution result.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="distributionStatus" minOccurs="1" type="resultType">
        <xsd:annotation>
          <xsd:documentation>The distribution status - set to SUCCESS or FAIL  When set to FAIL, the dstributionError tag will be populated with the error of failure.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="distributionError" minOccurs="0" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Contains reason why advisory message distribution was unsuccessful.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="resultType">
    <xsd:annotation>
      <xsd:documentation>The type of result.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="SUCCESS"/>
      <xsd:enumeration value="FAIL"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="tmiRequestType">
    <xsd:annotation>
      <xsd:documentation>The type of TMI to retrieve the TMI ID for.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="REROUTE"/>
      <xsd:enumeration value="CTOP"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="destinationType">
    <xsd:annotation>
      <xsd:documentation>List of destination.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="destinationType" minOccurs="1" type="addressType">
        <xsd:annotation>
          <xsd:documentation>The type of destination address.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="destinationAddress" minOccurs="1" type="addressDestinationType">
        <xsd:annotation>
          <xsd:documentation>Destination address to receive the advisory/general message.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="addressType">
    <xsd:annotation>
      <xsd:documentation>Destination type - ETMS, Nadin, Arinc.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="ETMS"/>
      <xsd:enumeration value="NADIN"/>
      <xsd:enumeration value="ARINC"/>
      <xsd:enumeration value="AIRLINE"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="advisoryMessageType">
    <xsd:sequence>
      <xsd:element name="advisoryNumber" type="advisoryNumType">
        <xsd:annotation>
          <xsd:documentation>The advisory number.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="advisoryType" minOccurs="0" type="messageCategoryType">
        <xsd:annotation>
          <xsd:documentation>The advisory type - ex) CDM AFP, CDM AFP COMPRESSION, CDM AFP CANCEL, CDM GDP, CDM GDP COMPRESSION, CDM GDP ADJUSTMENT, CDM GDP CANCEL, CDM GS, GS,  GS CANCEL, REROUTE, REROUTE CANCEL, CTOP, CTOP CANCEL, WEATHER, ROUTE, PLAYBOOK, CDR, VOLCANIC ACTIVITY, FCA, FEA, INFORMATIONAL, NAT, Special Operations, NRP SUSPENSIONS, SHUTTLE ACTIVITY, MISCELLANEOUS.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="origin" type="originNameType">
        <xsd:annotation>
          <xsd:documentation>Origin that produced the advisory.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="effectivePeriod" minOccurs="0" type="timePeriodType">
        <xsd:annotation>
          <xsd:documentation>The effective time period of the advisory.  The effective time is usually located at the bottom of the Advisory/General message.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="validTimePeriod" type="validPeriodType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The valid period of the advisory message can be set in the following format: ddhhmm in both the start and end times 301800 - 010200 or ddhhmm in just the start time and a string value in the end time 301800 - "AND LATER".</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="advisoryTitle" type="titleType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The advisory title.  The title will contain the following:ATCSCC ADVZY 001 EWR/ZNY 06/09/2015 CDM GROUND STOP.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="advisoryText" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>The advisory text in XML format.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="advisory" type="advisoryMessageDataType">
        <xsd:annotation>
          <xsd:documentation>The Advisory message.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="messageSignature" type="senderType" minOccurs="1">
        <xsd:annotation>
          <xsd:documentation>The last line in the advisory contains the message signature.15/06/23 15:02 DCCOPS./nfs/lxstn21 555-555-5555</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="timePeriodType">
    <xsd:sequence>
      <xsd:element name="startTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Start time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="endTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>End Time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="advisoryNumType">
    <xsd:annotation>
      <xsd:documentation>Advisory number.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="[0-9]{3}"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="originNameType">
    <xsd:annotation>
      <xsd:documentation>The origin entity that produced the Advisory.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="ATCSCC"/>
      <xsd:enumeration value="NAV CANADA"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="generalMessageType">
    <xsd:sequence>
      <xsd:element name="origin" type="originNameType">
        <xsd:annotation>
          <xsd:documentation>Origin that produced the general message.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="facilities" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Facilities associated with the general message.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="effectivePeriod" type="timePeriodType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The effective time period of the general message.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="validPeriod" minOccurs="0" type="validPeriodType">
        <xsd:annotation>
          <xsd:documentation>The valid period of the general message can be set in the following format: ddhhmm in both the start and end times 301800 - 010200 or ddhhmm in just the start time and a string value in the end time 301800 - "AND LATER".</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="generalTitle" type="titleType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Title of the general message.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="generalText" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Contents of the general message.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="messageSignature" type="senderType" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="advMessageType">
    <xsd:choice>
      <xsd:element name="advisoryMessage" type="advisoryMessageType">
        <xsd:annotation>
          <xsd:documentation>A message received from a center that will contain advisory information.  This message will include an advisory number.  Upon receipt of this message, a list of destinations will be included for this message to be distributed to.  This message will also be sent to NTML, OIS, and NEMS.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="generalMessage" type="generalMessageType">
        <xsd:annotation>
          <xsd:documentation>A message received from any center that will contain general information.  Upon receipt of this message, it will contain a list of addresses that this message will be distributed to.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="senderType">
    <xsd:sequence>
      <xsd:element name="dateSent" type="xsd:dateTime" minOccurs="1">
        <xsd:annotation>
          <xsd:documentation>The date and time the advisory was sent.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="sentByName" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The site name that issued the advisory.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="sentByPhone" minOccurs="0" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>If included, the phone number of the user who issued the advisory.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="rerouteAdvisoryType">
    <xsd:sequence>
      <xsd:element name="rerouteNameType" type="rerouteType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Reroute Type - RMD, FYI, RQD, and PLN.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="constrainedArea" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Defines the area of constraint for which the reroute was created.  Usually set as a center id.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="flightStatus" type="flightStatusType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The flight status that the reroute will apply to - ALL_FLIGHTS, AIRBORNE, or NON-AIRBORNE.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="validPeriod" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The valid period of the advisory in string format ex) ETD 301800 to 010200.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="rerouteType">
    <xsd:annotation>
      <xsd:documentation>The type of Reroute.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="RMD"/>
      <xsd:enumeration value="RQD"/>
      <xsd:enumeration value="PLN"/>
      <xsd:enumeration value="FYI"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="titleType">
    <xsd:sequence>
      <xsd:element name="facId" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The Facility ID that is included in the title after the advisory number - ATCSCC ADVZY 001 EWR/ZNY 06/09/2015 CDM GROUND STOP.  facid will be EWR/ZNY.For a CTOP - the Facility ID will be set to the CTOP short name - ATCSCC ADVZY 001 CTP001 06/09/2015 ACTUAL CTOP.  facid will be CTP001.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="titleDate" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The date that is included in the title after the advisory number - ATCSCC ADVZY 001 EWR/ZNY 06/09/2015 CDM GROUND STOP.  titleDate will be 06/09/2015.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="title" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The string that is included in the title after the date - ATCSCC ADVZY 001 EWR/ZNY 06/09/2015 CDM GROUND STOP.  title will be CDM GROUND STOP.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="gdpAfpType">
    <xsd:sequence>
      <xsd:element name="programRate" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Program rate for GDP and AFP only.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="flightsInclude" minOccurs="0" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Scope of the flights to include in the delay program.  Not included in a Ground Stop or CTOP.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="delayAssignmentMode" type="delayAssignmentModeType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>The type of GDP or AFP program - UDP, DAS, or GAAP.  Not used for Ground Stop or CTOP.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="cumTimePeriod" type="timePeriodType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Cumulative time value of the delay program. </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="delayAssign" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Holds the center for which the Delay Assignment Table Applies to for a GDP only</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="maxDelay" type="xsd:int" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Maximum delay value used for AFP and GDP only.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="avgDelay" type="xsd:int" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Average ground delay used for GDP, AFP, and CTOP only.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="programComment" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Additional comments or remarks entered when issuing the delay program.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:simpleType name="flightStatusType">
    <xsd:annotation>
      <xsd:documentation>The type flight status the Reroute applies to for a flight.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="ALL_FLIGHTS"/>
      <xsd:enumeration value="AIRBORNE"/>
      <xsd:enumeration value="NOT_AIRBORNE"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="delayAssignmentModeType">
    <xsd:annotation>
      <xsd:documentation>The type GDP/AFP program.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="UDP"/>
      <xsd:enumeration value="DAS"/>
      <xsd:enumeration value="GAAP"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="groundStopType">
    <xsd:sequence>
      <xsd:element name="elementType" type="dpElementType">
        <xsd:annotation>
          <xsd:documentation>Element type - airport or fca based program.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="extensionProbability" type="xsd:string" minOccurs="0"/>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="delayProgramAdvisoryType">
    <xsd:choice>
      <xsd:element name="ctop">
        <xsd:annotation>
          <xsd:documentation>Advisory information for a CTOP.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:complexContent>
            <xsd:extension base="ctopType"/>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="delayProgram" type="delayProgramType"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="ctopType">
    <xsd:sequence>
      <xsd:element name="programComment" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Additional comments or remarks entered when issuing the delay program.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="fcasIncluded" type="ctopFcaType" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>Names all of the FCAs included in the CTOP.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="tmiFacilitiesType">
    <xsd:sequence>
      <xsd:element name="distance" type="xsd:int" minOccurs="0" maxOccurs="1">
        <xsd:annotation>
          <xsd:documentation>The distance in nautical miles for departure flights that would be included in a delay program.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="addDepartureFacilities" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List any additionaly departure facilities that need to be included in an AFP or GDP only.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="exemptDepartureFacilities" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List departure facilities that may be exempt from the GDP or AFP only.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="includedCanadianAirports" minOccurs="0" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>List of Canadian departure facilities to include as part of the GDP, AFP, or CTOP only.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ctopFcaType">
    <xsd:sequence>
      <xsd:element name="fcaName" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>FCA Name that is included in the CTOP.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="fcaTime" type="timePeriodType">
        <xsd:annotation>
          <xsd:documentation>FCA start time.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="advisoryMessageDataType">
    <xsd:sequence>
      <xsd:element name="facilities" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>List of facilities affected by the Advisory/General message.For the following TMIs the facilities element tag will be set in the following matter:For reroute this will be set to the facilities listed in Facilities Included.For AFPs, this will be set to the facilities listed in the Departure Scope.For GDPs, this will be set to the facilities listed in the Departure Scope.For GS, this will be set to the facilities listed in Departure Facilities Included.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:choice>
        <xsd:element name="generalAdvisory">
          <xsd:annotation>
            <xsd:documentation>General Advisory sent in from the EMAIL Gui.</xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="tmiAdvisory">
          <xsd:annotation>
            <xsd:documentation>Advisory message that is sent as a result of a TMI being issued.Reroute, GDP, AFP, GS, or CTOP.</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="tmiCommon" type="tmiCommonType">
                <xsd:annotation>
                  <xsd:documentation>Common TMI elements that are used with Reroute, GDP, AFP, GS, and CTOP advisory messages.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element name="tmiAdvisoryData" type="advTmiType">
                <xsd:annotation>
                  <xsd:documentation>Reroute, GDP, AFP, GS, and CTOP Advisory data.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="delayProgramType">
    <xsd:sequence>
      <xsd:element name="adlTime" type="xsd:string" minOccurs="1">
        <xsd:annotation>
          <xsd:documentation>Time of ADL that the program was created from - format 1700z. </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="departureScope" minOccurs="0">
        <xsd:complexType>
          <xsd:complexContent>
            <xsd:extension base="tmiFacilitiesType">
              <xsd:attribute name="tier" type="xsd:string">
                <xsd:annotation>
                  <xsd:documentation>The different tiers for scope of a GDP/GS program</xsd:documentation>
                </xsd:annotation>
              </xsd:attribute>
            </xsd:extension>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:choice>
        <xsd:element name="groundStop">
          <xsd:annotation>
            <xsd:documentation>Advisory information for a Ground Stop.</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:complexContent>
              <xsd:extension base="groundStopType"/>
            </xsd:complexContent>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="gdpAfp">
          <xsd:annotation>
            <xsd:documentation>Advisory information for a GDP/AFP.  Also other type of TMI that can occur when a GDP or AFP is in place, Compression or Blanket.</xsd:documentation>
          </xsd:annotation>
          <xsd:complexType>
            <xsd:complexContent>
              <xsd:extension base="gdpAfpType"/>
            </xsd:complexContent>
          </xsd:complexType>
        </xsd:element>
      </xsd:choice>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="advTmiType">
    <xsd:choice>
      <xsd:element name="rerouteAdvisory">
        <xsd:annotation>
          <xsd:documentation>Reroute information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:complexContent>
            <xsd:extension base="rerouteAdvisoryType">
              <xsd:attribute name="rerouteCancel" type="xsd:boolean">
                <xsd:annotation>
                  <xsd:documentation>Boolean to indicate if the reroute has been canceled.</xsd:documentation>
                </xsd:annotation>
              </xsd:attribute>
            </xsd:extension>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="delayProgramAdvisory">
        <xsd:annotation>
          <xsd:documentation>Delay Program information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:complexContent>
            <xsd:extension base="delayProgramAdvisoryType">
              <xsd:attribute name="programStatus" type="programStatusType">
                <xsd:annotation>
                  <xsd:documentation>Indicates if the program is an Actual or Proposed program.</xsd:documentation>
                </xsd:annotation>
              </xsd:attribute>
              <xsd:attribute name="programPurge" type="xsd:boolean">
                <xsd:annotation>
                  <xsd:documentation>Boolean to indicate if the program has been purged.</xsd:documentation>
                </xsd:annotation>
              </xsd:attribute>
            </xsd:extension>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
    </xsd:choice>
  </xsd:complexType>
  <xsd:complexType name="siteNodeType">
    <xsd:sequence>
      <xsd:element name="siteName" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Site name of where the message will be sent to.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="node" type="xsd:string" minOccurs="1" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>Contains a list of all the nodes the messages are to be sent to.  This is only used for ETMS messages.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="addressDestinationType">
    <xsd:choice>
      <xsd:element name="address" type="xsd:string" maxOccurs="unbounded"/>
      <xsd:element name="siteNode" type="siteNodeType" maxOccurs="unbounded"/>
    </xsd:choice>
  </xsd:complexType>
  <xsd:simpleType name="dpElementType">
    <xsd:annotation>
      <xsd:documentation>The type element that program was issued for.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="APT"/>
      <xsd:enumeration value="FCA"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="messageCategoryType">
    <xsd:annotation>
      <xsd:documentation>The different advisory message categories.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="CDM AFP"/>
      <xsd:enumeration value="CDM AFP COMPRESSION"/>
      <xsd:enumeration value="CDM AFP CANCEL"/>
      <xsd:enumeration value="CDM GDP"/>
      <xsd:enumeration value="CDM GDP COMPRESSION"/>
      <xsd:enumeration value="CDM GDP ADJUSTMENT"/>
      <xsd:enumeration value="CDM GDP CANCEL"/>
      <xsd:enumeration value="CDM GS"/>
      <xsd:enumeration value="GS"/>
      <xsd:enumeration value="GS CANCEL"/>
      <xsd:enumeration value="REROUTE"/>
      <xsd:enumeration value="REROUTE CANCEL"/>
      <xsd:enumeration value="CTOP"/>
      <xsd:enumeration value="CTOP CANCEL"/>
      <xsd:enumeration value="OTHER"/>
      <xsd:enumeration value="ROUTE"/>
      <xsd:enumeration value="PLAYBOOK"/>
      <xsd:enumeration value="CDR"/>
      <xsd:enumeration value="SPECIAL OPERATIONS"/>
      <xsd:enumeration value="NRP SUSPENSIONS"/>
      <xsd:enumeration value="VOLCANIC ACTIVITY"/>
      <xsd:enumeration value="NAT"/>
      <xsd:enumeration value="SPACE LAUNCH"/>
      <xsd:enumeration value="FCA"/>
      <xsd:enumeration value="FEA"/>
      <xsd:enumeration value="INFORMATIONAL"/>
      <xsd:enumeration value="MISCELLANEOUS"/>
      <xsd:enumeration value="OPERATIONS PLANNING"/>
      <xsd:enumeration value="TCA"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="programStatusType">
    <xsd:annotation>
      <xsd:documentation>The status of the program ACTUAL/PROPOSED.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="PROPOSED"/>
      <xsd:enumeration value="ACTUAL"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="tmiCommonType">
    <xsd:sequence>
      <xsd:element name="tmiName" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Name of the TMI - Reroute name, GDP, AFP, GS, CTOP name.  Only 4 characters is allowed for the GDP and GS name, 6 characters for a CTOP and AFP name, and a minimum of 2 characters up to 64 characters for a Reroute name.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="tmiTime" type="timePeriodType" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>TMI time period.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="tmiReason" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>Reason for TMI.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="validPeriodType">
    <xsd:sequence>
      <xsd:element name="validStartTime" type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation>Start time of the affected valid period.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="validEndTime" type="xsd:string" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>End time of the affected valid period or a string value that describes the end period.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
</xsd:schema>
